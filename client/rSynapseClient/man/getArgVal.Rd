## The function is currently defined as
function (argName) 
{
    constants <- new("SynapseWorkflowConstants")
    args <- commandArgs(trailingOnly = TRUE)
    indx <- which(args == argName)
    if (length(indx) == 1) 
        return(args[indx + 1])
    stop(constants@kUsage, indx)
  }
}
