<configuration>
	<columns>
		<!-- The basic columns -->
		<column id="dataset.id">
			<type>String</type>
			<display>GUID</display>
			<description>The GUID of this dataset.
			</description>
		</column>
		<column id="dataset.name">
			<type>String</type>
			<display>Dataset Name</display>
			<description>Name of the Dataset and link to more information
			</description>
		</column>
		<column id="dataset.description">
			<type>String</type>
			<display>Description</display>
			<description>The description of this datasets</description>
		</column>
		<column id="dataset.createdBy">
			<type>String</type>
			<display>Investigator</display>
			<description>The person that created this dataset</description>
		</column>
		<column id="dataset.modifiedOn">
			<type>Long</type>
			<display>Modified On</display>
			<description>The date this dataset was last changed</description>
		</column>
		<column id="dataset.status">
			<type>String</type>
			<display>Status</display>
			<description>The status of this dataset. Can be one of the follwing:
				{"Available", "In Transition", "Future"}</description>
		</column>
		<column id="dataset.releaseDate">
			<type>Long</type>
			<display>Release Date</display>
			<description>The date this dataset was released</description>
		</column>
		<column id="dataset.version">
			<type>String</type>
			<display>Current Version</display>
			<description>The current version of this datasets</description>
		</column>
		<column id="dataset.annotations">
			<type>String</type>
			<display>Annotations link</display>
			<description>Link to this dataset's annotations</description>
		</column>
		<column id="dataset.uri">
			<type>String</type>
			<display>URI</display>
			<description>This dataset's URI</description>
		</column>
		<column id="dataset.etag">
			<type>String</type>
			<display>e-Tag</display>
			<description>The concurrent e-Tag, which must be provide to update a
				dataset</description>
		</column>
		<column id="dataset.layer">
			<type>String</type>
			<display>Layer URL</display>
			<description>The link to this dataset's layers</description>
		</column>
		<column id="dataset.disease">
			<type>StringArray</type>
			<display>Disease</display>
			<description>What disease is this dataset about</description>
		</column>
		<column id="dataset.SampleSource">
			<type>StringArray</type>
			<display>Sample Source</display>
			<description>Sample source</description>
		</column>
		<column id="dataset.tissueType">
			<type>StringArray</type>
			<display>Tissue Type</display>
			<description>Tissue</description>
		</column>
		<column id="dataset.Institution">
			<type>StringArray</type>
			<display>Institution</display>
			<description>Institution</description>
		</column>
		<column id="dataset.species">
			<type>StringArray</type>
			<display>Species</display>
			<description>Species</description>
		</column>
		<column id="dataset.Study_Size">
			<type>StringArray</type>
			<display>Study Size</display>
			<description>Study Size</description>
		</column>
		<column id="dataset.numSamples">
			<type>IntegerArray</type>
			<display>Number of Samples</display>
			<description>Number of Samples</description>
		</column>
		<column id="dataset.pubmedId">
			<type>LongArray</type>
			<display>Reference / PubMed ID</display>
			<description>Reference / PubMed ID</description>
		</column>
		
		<!-- These are for layers -->
		<column id="layer.id">
			<type>String</type>
			<display>GUID</display>
			<description>The GUID of this layer.
			</description>
		</column>
		<column id="layer.name">
			<type>String</type>
			<display>Name</display>
			<description>Layer Name</description>
		</column>
		<column id="layer.numSamples">
			<type>Long</type>
			<display>Number of Samples</display>
			<description>Number of Samples in this layer</description>
		</column>
		<column id="layer.version">
			<type>String</type>
			<display>Version</display>
			<description>Version</description>
		</column>
		<column id="layer.parentId">
			<type>String</type>
			<display>Parent</display>
			<description>Parent</description>
		</column>
		<column id="staticdisplay.download">
			<type>String</type>
			<display>Download</display>
			<description>download</description>
		</column>		
		<column id="layer.createdBy">
			<type>String</type>
			<display>Investigator</display>
			<description>The person that created this layer</description>
		</column>
		<column id="layer.modifiedOn">
			<type>Long</type>
			<display>Modified On</display>
			<description>The date this layer was last modified</description>
		</column>
	
		
		
		<!-- Project columns -->
		<column id="project.id">
			<type>String</type>
			<display>Project ID</display>
			<description>Project ID</description>
		</column>		
		<column id="project.name">
			<type>String</type>
			<display>project Name</display>
			<description>Name of the project</description>
		</column>
		<column id="project.description">
			<type>String</type>
			<display>Description</display>
			<description>The description of this project</description>
		</column>
		<column id="project.createdBy">
			<type>String</type>
			<display>Investigator</display>
			<description>The person that created this project</description>
		</column>
		<column id="project.modifiedOn">
			<type>Long</type>
			<display>Modified On</display>
			<description>The date this project was last modified</description>
		</column>
	
		<!-- Analysis columns -->
		<column id="analysis.id">
			<type>String</type>
			<display>Analysis ID</display>
			<description>Analysis ID</description>
		</column>		
		<column id="analysis.name">
			<type>String</type>
			<display>Analysis Name</display>
			<description>Name of the analysis</description>
		</column>
		<column id="analysis.description">
			<type>String</type>
			<display>Description</display>
			<description>The description of this analysis</description>
		</column>
		<column id="analysis.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this analysis</description>
		</column>
		<column id="analysis.modifiedOn">
			<type>Long</type>
			<display>Modified On</display>
			<description>The date this analysis was last modified</description>
		</column>

		<!-- Step columns -->
		<column id="step.id">
			<type>String</type>
			<display>Step ID</display>
			<description>Step ID</description>
		</column>		
		<column id="step.name">
			<type>String</type>
			<display>Step Name</display>
			<description>Name of the step</description>
		</column>
		<column id="step.commandLine">
			<type>String</type>
			<display>Command Line</display>
			<description>The command line of this step</description>
		</column>
		<column id="step.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this step</description>
		</column>
		<column id="step.modifiedOn">
			<type>Long</type>
			<display>Modified Date </display>
			<description>The date this step was last changed</description>
		</column>
		
		<!-- Code columns -->
		<column id="code.id">
			<type>String</type>
			<display>Code ID</display>
			<description>Code ID</description>
		</column>		
		<column id="code.name">
			<type>String</type>
			<display>Code Name</display>
			<description>Name of this Code</description>
		</column>
		<column id="code.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this code</description>
		</column>
		<column id="code.modifiedOn">
			<type>Long</type>
			<display>Modified Date </display>
			<description>The date this code was last changed</description>
		</column>
		
		<!-- Link columns -->
		<column id="link.id">
			<type>String</type>
			<display>Link ID</display>
			<description>Link ID</description>
		</column>		
		<column id="link.name">
			<type>String</type>
			<display>Link Name</display>
			<description>Name of this Link</description>
		</column>
		<column id="link.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this link</description>
		</column>
		<column id="link.modifiedOn">
			<type>Long</type>
			<display>Modified Date </display>
			<description>The date this link was last changed</description>
		</column>
		
		<!-- ExpressionData -->
		<column id="expressiondata.id">
			<type>String</type>
			<display>Data ID</display>
			<description>Data ID</description>
		</column>		
		<column id="expressiondata.name">
			<type>String</type>
			<display>Data Name</display>
			<description>Name of this data</description>
		</column>
		<column id="expressiondata.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this data</description>
		</column>
		<column id="expressiondata.modifiedOn">
			<type>Long</type>
			<display>Modified Date </display>
			<description>The date this data was last changed</description>
		</column>
		
		<!-- genotypeData -->
		<column id="genotypedata.id">
			<type>String</type>
			<display>Data ID</display>
			<description>Data ID</description>
		</column>		
		<column id="genotypedata.name">
			<type>String</type>
			<display>Data Name</display>
			<description>Name of this data</description>
		</column>
		<column id="genotypedata.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this data</description>
		</column>
		<column id="genotypedata.modifiedOn">
			<type>Long</type>
			<display>Modified Date </display>
			<description>The date this data was last changed</description>
		</column>
		
		<!-- phenotypeData -->
		<column id="phenotypedata.id">
			<type>String</type>
			<display>Data ID</display>
			<description>Data ID</description>
		</column>		
		<column id="phenotypedata.name">
			<type>String</type>
			<display>Data Name</display>
			<description>Name of this data</description>
		</column>
		<column id="phenotypedata.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this data</description>
		</column>
		<column id="phenotypedata.modifiedOn">
			<type>Long</type>
			<display>Modified Date </display>
			<description>The date this data was last changed</description>
		</column>

		<!-- robject -->
		<column id="robject.id">
			<type>String</type>
			<display>Data ID</display>
			<description>Data ID</description>
		</column>		
		<column id="robject.name">
			<type>String</type>
			<display>Data Name</display>
			<description>Name of this data</description>
		</column>
		<column id="robject.createdBy">
			<type>String</type>
			<display>Created By</display>
			<description>The person that created this data</description>
		</column>
		<column id="robject.modifiedOn">
			<type>Long</type>
			<display>Modified Date </display>
			<description>The date this data was last changed</description>
		</column>
		
		<!-- The more complex/composite columns -->
		
		<!-- A dynamic relative URL will be built for datatsets using the ID column -->
		<url-template id="dataset.localUrl">
			<type>String</type>
			<urlTemplate>Synapse:{dataset.id}</urlTemplate>
		</url-template>
		<!-- Acts as the dataset name column  -->
		<link-column id="dataset.NameLink">
			<display reference="dataset.name" />
			<url reference="dataset.localUrl" />
		</link-column>
		
		<!-- The modified on date -->
		<date-column id="dataset.modifiedOn">
			<baseColumn reference="dataset.modifiedOn"/>
		</date-column>

		<!-- A dynamic relative URL will be built for layers using the ID column -->
		<url-template id="layer.Url">
			<type>String</type>
			<urlTemplate>Synapse:{layer.id}</urlTemplate>
		</url-template>
		<!-- Acts as the layer name column  -->
		<link-column id="layer.NameLink">
			<display reference="layer.name" />
			<url reference="layer.Url" />
		</link-column>
		
		<!-- A dynamic relative URL will be built for downloading layers using the ID column -->
		<url-template id="layer.DownloadUrl">
			<type>String</type>
			<urlTemplate>Synapse:{layer.id}</urlTemplate>
		</url-template>
		<!-- Acts as the layer name column  -->
		<link-column id="layer.DownloadLink">
			<display reference="staticdisplay.download" />
			<url reference="layer.DownloadUrl" />
		</link-column>

		<!-- A dynamic relative URL will be built for projects using the ID column -->
		<url-template id="project.localUrl">
			<type>String</type>
			<urlTemplate>Synapse:{project.id}</urlTemplate>
		</url-template>
		<!-- Acts as the project name column  -->
		<link-column id="project.NameLink">
			<display reference="project.name" />
			<url reference="project.localUrl" />
		</link-column>	
		
		<!-- A dynamic relative URL will be built for analyses using the ID column -->
		<url-template id="analysis.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{analysis.id}</urlTemplate>
		</url-template>
		<!-- Acts as the analysis name column  -->
		<link-column id="analysis.NameLink">
			<display reference="analysis.name" />
			<url reference="analysis.LocalUrl" />
		</link-column>
		
		<!-- A dynamic relative URL will be built for steps using the ID column -->
		<url-template id="step.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{step.id}</urlTemplate>
		</url-template>
		
		<!-- Acts as the step name column  -->
		<link-column id="step.NameLink">
			<display reference="step.name" />
			<url reference="step.LocalUrl" />
		</link-column>
			
		<!-- A dynamic relative URL will be built for Codes using the ID column -->
		<url-template id="code.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{code.id}</urlTemplate>
		</url-template>
		
		<!-- Acts as the code name column  -->
		<link-column id="code.NameLink">
			<display reference="code.name" />
			<url reference="code.LocalUrl" />
		</link-column>
			
		<!-- A dynamic relative URL will be built for Link using the ID column -->
		<url-template id="link.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{link.id}</urlTemplate>
		</url-template>
		
		<!-- Acts as the code name column  -->
		<link-column id="link.NameLink">
			<display reference="link.name" />
			<url reference="link.LocalUrl" />
		</link-column>
			
		<!-- A dynamic relative URL will be built for ExpressionData using the ID column -->
		<url-template id="expressiondata.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{expressiondata.id}</urlTemplate>
		</url-template>
		
		<!-- Acts as the Data name column  -->
		<link-column id="expressiondata.NameLink">
			<display reference="expressiondata.name" />
			<url reference="expressiondata.LocalUrl" />
		</link-column>
			
		<!-- A dynamic relative URL will be built for genotypeData using the ID column -->
		<url-template id="genotypedata.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{genotypedata.id}</urlTemplate>
		</url-template>
		
		<!-- Acts as the Data name column  -->
		<link-column id="genotypedata.NameLink">
			<display reference="genotypedata.name" />
			<url reference="genotypedata.LocalUrl" />
		</link-column>
			
		<!-- A dynamic relative URL will be built for phenotypeData using the ID column -->
		<url-template id="phenotypedata.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{phenotypedata.id}</urlTemplate>
		</url-template>
		
		<!-- Acts as the Data name column  -->
		<link-column id="phenotypedata.NameLink">
			<display reference="phenotypedata.name" />
			<url reference="phenotypedata.LocalUrl" />
		</link-column>
			
		<!-- A dynamic relative URL will be built for phenotypeData using the ID column -->
		<url-template id="robject.LocalUrl">
			<type>String</type>
			<urlTemplate>Synapse:{robject.id}</urlTemplate>
		</url-template>
		
		<!-- Acts as the Data name column  -->
		<link-column id="robject.NameLink">
			<display reference="robject.name" />
			<url reference="robject.LocalUrl" />
		</link-column>

		<!-- The modified on date -->
		<date-column id="layer.modifiedOn">
			<baseColumn reference="layer.modifiedOn"/>
		</date-column>

		<!-- The modified on date -->
		<date-column id="project.modifiedOn">
			<baseColumn reference="project.modifiedOn"/>
		</date-column>

		<!-- The created on date -->
		<date-column id="analysis.modifiedOn">
			<baseColumn reference="analysis.modifiedOn"/>
		</date-column>

		<!-- The created on date -->
		<date-column id="step.modifiedOn">
			<baseColumn reference="step.modifiedOn"/>
		</date-column>
		
		<!-- The created on date -->
		<date-column id="code.modifiedOn">
			<baseColumn reference="code.modifiedOn"/>
		</date-column>

		<!-- The created on date -->
		<date-column id="link.modifiedOn">
			<baseColumn reference="link.modifiedOn"/>
		</date-column>
		
		<!-- The created on date -->
		<date-column id="expressiondata.modifiedOn">
			<baseColumn reference="expressiondata.modifiedOn"/>
		</date-column>
		
		<!-- The created on date -->
		<date-column id="genotypedata.modifiedOn">
			<baseColumn reference="genotypedata.modifiedOn"/>
		</date-column>
		
		<!-- The created on date -->
		<date-column id="phenotypedata.modifiedOn">
			<baseColumn reference="phenotypedata.modifiedOn"/>
		</date-column>
		
		<!-- The created on date -->
		<date-column id="robject.modifiedOn">
			<baseColumn reference="robject.modifiedOn"/>
		</date-column>
		
	</columns>
</configuration>