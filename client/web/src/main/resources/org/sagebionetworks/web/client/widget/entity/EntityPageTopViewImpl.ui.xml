<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />

	<g:HTMLPanel>
		<div id="content" class="clearfix">

			<ul class="breadcrumbs">
				<li class="breadcrumb">
					<a href="#">Home</a>
					>
					<a href="#DatasetsHome:0">All Datasets</a>
					>
					<span ui:field="breadcrumbTitleSpan" />
				</li>
			</ul>
			<div id="privacy_setting" class="right">
				<table>
					<tr valign="bottom">
						<td style="padding-right: 10px">
							<g:SimplePanel ui:field="adminPanel" />
						</td>
						<td>
							<g:SimplePanel ui:field="accessPanel" />
							<span ui:field="accessSpan" />
						</td>
					</tr>
				</table>
			</div>
			<div class="data_info">
				<span class="info_head">
					<h2>
						<span ui:field="titleSpan" />
						<br />
						<span ui:field="synapseIdSpan" style="font-size: 60%" />
					</h2>

					<span class="data_options">
							<g:SimplePanel ui:field="bannerRightTopSlot" />
							<g:SimplePanel ui:field="bannerRightBottomSlot" /> 
					</span>
				</span>
				<span class="info_text">
					<g:FlowPanel ui:field="overviewPanel" />
					<br />
					<br />
					<div ui:field="rClientCodeDiv" />
				</span>
				<span style="float:right;">
					<g:SimplePanel ui:field="annotationsPanel" />
				</span>
			</div>
		</div>

	</g:HTMLPanel>
</ui:UiBinder>