<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />

	<g:HTMLPanel>
		<div id="content" class="clearfix">
			<div id="main" role="main">
				<ul class="breadcrumbs">
					<li class="breadcrumb">
						<g:SimplePanel ui:field="breadcrumbsPanel"/>
					</li>
				</ul>			
				
 				<div id="privacy_setting" class="right">
					<table>
						<tr valign="bottom">
							<td style="padding-right: 10px">
								<g:SimplePanel ui:field="actionMenuPanel" />
							</td>
						</tr>
					</table>
				</div>
				
  				<div class="portal">		
					<!-- <g:SimplePanel ui:field="portalPanel" styleName="portal rounded dropshaddow"/>  -->							
					<g:SimplePanel ui:field="portalPanel"/>	
					<g:SimplePanel ui:field="portalPanelSingleCol"/>							
					<g:SimplePanel ui:field="portalPanelThreeCol"/>
				</div>						
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>

