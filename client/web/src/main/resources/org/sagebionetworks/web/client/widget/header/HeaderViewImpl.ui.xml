<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />
	<ui:with field='sageImages' type='org.sagebionetworks.web.client.SageImageBundle' />
	<g:HTMLPanel>

		<div class="span-24 notopmargin">
			<div class="span-16 logo">
				<a href="#">
					<g:Image resource="{sageImages.sageLogoAndTitle}" />
				</a>
			</div>
			<div class="span-8 last">
				<h5 style="text-align: right;">
					<span ui:field="userName" />
				</h5>
				<div class="menu-right"></div>
				<ul class="sf-menu">
					<li>
						<g:Anchor ui:field="topRightLink1" />
					</li>
					<li>
						<g:Hyperlink ui:field="topRightLink2" />
					</li>
				</ul>
				<div class="menu-left"></div>
			</div>
			<div class="span-24 notopmargin">
				<div class="span-16 notopmargin">&nbsp;</div>
				<div class="span-8 last notopmargin" style="text-align: right;">
					<g:SimplePanel ui:field="searchBoxPanel" />
				</div>
			</div>
		</div>

	</g:HTMLPanel>
</ui:UiBinder>