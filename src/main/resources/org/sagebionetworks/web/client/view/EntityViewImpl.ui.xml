<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view">

	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.rowTop {
			vertical-align: top;
		}
	</ui:style>

	<g:HTMLPanel>
		<!-- HEADER -->
		<div class="color-line"></div>
		<g:SimplePanel ui:field="header" />
		<div class="separator separator-header-page"></div>
		<g:Image ui:field="entityBackgroundImage" addStyleNames="entityBackgroundImage" />
		<g:SimplePanel ui:field="entityPageTopPanel"/>
        <g:SimplePanel ui:field="synAlertContainer"/>
        <div class="clear"></div>
		<g:SimplePanel ui:field="footer" />
	</g:HTMLPanel>
</ui:UiBinder> 