<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
>
  <g:HTMLPanel>
    <div>
      <bh:Div addStyleNames="pageHeader">
        <b:Heading addStyleNames="pageHeaderTitle" size="H3" text="Following" />
        <bh:Paragraph addStyleNames="description">
          These are discussion threads and forums that you are subscribed
          to. You will receive an email when someone responds or
          creates a thread.
        </bh:Paragraph>
      </bh:Div>
      <bh:Div ui:field="synAlertContainer" addStyleNames="container" />
      <w:ReactComponentDiv
        addStyleNames="pageContent margin-top-30"
        ui:field="reactContainer"
      />
    </div>
  </g:HTMLPanel>
</ui:UiBinder>
