<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view">
	<g:HTMLPanel>

		<div class="color-line"></div>
		<g:SimplePanel ui:field="header" />
		<div class="separator margin-top-10-imp"></div>
		<div class="margin-10">
			<div class="row margin-top-5">
				<div class="col-md-3" style="white-space: normal;">
					<g:SimplePanel ui:field="breadcrumbsPanel" />
				</div>
				<div class="col-md-9">
					<g:FlowPanel ui:field="editProfileButtonPanel" />
				</div>
			</div>

			<div class="row">
				<div class="col-md-3 col-sm-4 col-xs-12">
						<g:SimplePanel ui:field="picturePanel"/>
				</div>
				<div class="col-md-9 col-sm-8 col-xs-12">
					<g:SimplePanel ui:field="viewProfilePanel" />
				</div>
			</div>
			<div class="row margin-top-5">
				<div class="col-xs-12">
					<g:Anchor ui:field="showProfileLink" text="Show my full profile" />
				</div>
			</div>
			<div class="row margin-top-5">
			    <div class="col-xs-12">
				    <div class="row" ui:field="navtabContainer">
					    <ul class="nav nav-tabs entity-tabs margin-top-15">				        
						    <li class="entity-tab active margin-left-15" ui:field="projectsListItem"><g:Anchor styleName="text-align-center no-background" ui:field="projectsLink"/></li>
						    <!--  <li class="entity-tab" ui:field="challengesListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="challengesLink"/></li> -->
						    <li class="entity-tab" ui:field="teamsListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="teamsLink"/></li>
						    <!--  <li class="entity-tab" ui:field="messagesListItem" style="width: 157px"><g:Anchor styleName="text-align-center link no-background" ui:field="messagesLink"/></li> -->
						    <li class="entity-tab" ui:field="settingsListItem" style="width: 157px"><g:Anchor styleName="text-align-center link no-background" ui:field="settingsLink"/></li>
					    </ul>
					</div>
					<div class="row">
						<div class="tab-background padding-top-bottom-15-left-right-25" ui:field="projectsTabContainer">
							<div ui:field="createProjectUI" class="margin-bottom-10 row">
							    <div class="col-xs-12 col-sm-6">
									<div class="input-group">
										<g:TextBox ui:field="createProjectTextBox" styleName="form-control" />
										<span class="input-group-btn">
											<g:Button ui:field="createProjectButton" styleName="btn btn-default">Create Project</g:Button>
										</span>
									</div>
								</div>
								<div class="col-xs-12 col-sm-6" />
								<div class="col-xs-12 left">
								    <a href="#!ProjectsHome:0">What's a Project?</a>
								</div>
							</div>
							<g:FlowPanel ui:field="projectsTabContent" />
						</div>
					    <g:FlowPanel styleName="tab-background padding-top-bottom-15-left-right-25" ui:field="challengesTabContainer" />
						<div class="tab-background padding-15" ui:field="teamsTabContainer">
							<div ui:field="createTeamUI" class="margin-bottom-10 row">
							    <div class="col-xs-12 col-sm-6">
									<div class="input-group">
										<g:TextBox ui:field="createTeamTextBox" styleName="form-control" />
										<span class="input-group-btn">
											<g:Button ui:field="createTeamButton" styleName="btn btn-default">Create Team</g:Button>
										</span>
									</div>
								</div>
								<div class="col-xs-12 col-sm-6" />
							</div>
							<g:FlowPanel ui:field="teamsTabContent" />
						</div>
					    <g:FlowPanel styleName="tab-background padding-top-bottom-15-left-right-25" ui:field="messagesTabContainer" />
					    <g:FlowPanel styleName="tab-background padding-top-bottom-15-left-right-25" ui:field="settingsTabContainer" />
						
					</div>
				</div>
			</div>
			<div class="row margin-top-5">
			    <div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10 margin-bottom-20">
				<g:SimplePanel ui:field="updateWithLinkedInPanel" />
			</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10 margin-bottom-10">
					<g:SimplePanel ui:field="editPicturePanel"/>
				</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10 margin-bottom-20">
					<g:SimplePanel ui:field="editPictureButtonPanel"/>
				</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10">
					<g:SimplePanel ui:field="updateUserInfoPanel" />
				</div>
				<div class="col-md-offset-2 col-md-8 col-sm-offset-1 col-sm-10">
					<g:SimplePanel ui:field="certificatePanel" />
				</div>
			</div>
			<div class="row margin-top-5">				
				
			</div>
			<p>&nbsp;</p>
		</div>

		<!-- start footer -->
		<div class="clear"></div>
		<g:SimplePanel ui:field="footer" />
		<!-- end footer -->

	</g:HTMLPanel>
</ui:UiBinder> 