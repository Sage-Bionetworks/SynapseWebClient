<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view">
		
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.rowTop {
			vertical-align: top;
		}
	</ui:style>
		
	<g:HTMLPanel>

	<!-- start header -->
	<!-- MAIN CONTAINER -->
		<!-- HEADER -->
        <div class="color-line"></div>
        <div class="clear"></div>
    	<div class="container"> 		
			<g:SimplePanel ui:field="header" />
				<div class="span-24 separator"></div>
	        	<div class="span-24 notopmargin">	
	<!-- end header -->
					<div class="span-24 notopmargin" style="margin-top: 5px !important">
						<div class="span-20 notopmargin" style="white-space: normal;">
							<g:SimplePanel ui:field="breadcrumbsPanel"/>
						</div>
						<div class="span-4 last right notopmargin">
							<g:SimplePanel ui:field="editProfileButtonPanel" />
						</div>
					</div>
					
					<div class="span-24 notopmargin last">
						<div id="pictureCanvasId" class="span-5 inner-6 view notopmargin">
							<!-- profile picture, and edit photo (if visible) -->
							<center>
								<g:SimplePanel ui:field="profilePicturePanel" />
							</center>
							<div class="span-4 push-2 notopmargin">
								<g:SimplePanel ui:field="editPhotoButtonPanel" />
							</div>
						</div>
						<div class="span-18 notopmargin last">
							<g:SimplePanel ui:field="viewProfilePanel" />
						</div>
					</div>
					<div class="user-profile-linkedin-bar span-13 push-11 last notopmargin">
						<g:SimplePanel ui:field="updateWithLinkedInPanel" />
					</div>
					<div class="span-24 notopmargin last">
						<g:SimplePanel ui:field="updateUserInfoPanel" />
					</div>
					<p>&nbsp;</p>					
					
				</div>
					
<!-- start footer -->
            <div class="clear"></div>
        </div> <!-- close class="container" -->
		<g:SimplePanel ui:field="footer" />
<!-- end footer -->

	</g:HTMLPanel>
</ui:UiBinder> 