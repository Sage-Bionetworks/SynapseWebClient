<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<ui:with field='sageImageBundle'
		type='org.sagebionetworks.web.client.SageImageBundle' />
	<t:Table>
		<t:TableRow>
			<t:TableData paddingRight="2">
				<b:Icon ui:field="successIcon"
					addStyleNames="display-inline moveup-10" size="TIMES2" type="CHECK"
					color="GREEN" visible="false"></b:Icon>
				<b:Icon ui:field="failureIcon"
					addStyleNames="display-inline moveup-10" size="TIMES2" type="TIMES"
					color="RED" visible="false"></b:Icon>
			</t:TableData>
			<t:TableData paddingRight="2">
				<g:FlowPanel ui:field="questionContainer"
					addStyleNames="margin-bottom-40 margin-left-15">
					<b:Heading ui:field="questionHeader" size="H5"
						addStyleNames="inline-block control-label margin-right-10">
					</b:Heading>
					<bh:Div ui:field="helpContainer"
						addStyleNames="margin-left-9" />
				</g:FlowPanel>
			</t:TableData>
		</t:TableRow>
	</t:Table>



</ui:UiBinder> 