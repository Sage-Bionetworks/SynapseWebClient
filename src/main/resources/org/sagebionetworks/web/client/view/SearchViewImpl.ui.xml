<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view">
	<g:HTMLPanel>

		<div class="color-line"></div>
		<g:SimplePanel ui:field="header" />
		<div class="separator separator-header-page"></div>

		<div class="container margin-top-15">
			<div class="row">
				<div class="col-md-9 col-md-push-3">
					<div class="row">
						<div class="col-md-12">
							<g:SimplePanel ui:field="searchBoxPanel" />
						</div>
					</div>
					<div class="row">
						<div class="col-md-12">
							<g:SimplePanel ui:field="currentFacetsPanel" />
						</div>
					</div>
					<g:SimplePanel ui:field="resultsPanel" styleName="margin-top-15"/>
					<g:SimplePanel ui:field="paginationPanel" />
				</div>
				<div class="col-md-3 col-md-pull-9">
					<div class="well">
						<h5>Narrow Results</h5>
						<g:SimplePanel ui:field="facetPanel" />
					</div>
				</div>
				<div class="clear"></div>
			</div>
		</div>
		<g:SimplePanel ui:field="footer" />

	</g:HTMLPanel>
</ui:UiBinder> 