<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<bh:Div addStyleNames="SRC-card-grid-row margin-top-20">
			<bh:Div addStyleNames="SRC-grid-item" marginLeft="15"
				marginBottom="5" height="50px">
				<b:Heading size="H3" text="Teams" />
			</bh:Div>
			<bh:Div addStyleNames="SRC-grid-item" height="10px" />
			<bh:Div
				addStyleNames="SRC-grid-item flexcontainer-align-items-flex-end"
				marginTop="10" marginBottom="5" marginRight="15" height="50px">
				<bh:Div>
					<b:Icon ui:field="searchButton" type="SEARCH"
						addStyleNames="imageButton font-size-22 movedown-28 synapse-green left" />
					<b:TextBox ui:field="searchField" height="35px"
						width="100%" addStyleNames="inline-block padding-left-35"
						placeholder="Find teams" />
				</bh:Div>
			</bh:Div>
		</bh:Div>
		<g:SimplePanel ui:field="synAlertPanel" />
		<g:FlowPanel ui:field="mainContainer"
			addStyleNames="margin-top-10" />
		<g:SimplePanel ui:field="paginationPanel" />
	</bh:Div>
</ui:UiBinder> 