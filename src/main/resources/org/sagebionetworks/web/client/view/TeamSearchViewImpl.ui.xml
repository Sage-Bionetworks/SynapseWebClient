<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<g:HTMLPanel>
	    <div class="color-line"></div>
		<g:SimplePanel ui:field="header" />
		<div class="separator separator-header-page"></div>
		<div class="col-md-12 margin-top-15 margin-bottom-40"><h3>Search Teams</h3></div>
		<b:Container addStyleNames="margin-top-15">
		    <b:Row>
				<b:InputGroup>
				    <b:TextBox ui:field="searchField" addStyleNames="input-lg"/>
				    <b:InputGroupButton>
				        <b:Button ui:field="searchButton" icon="SEARCH" addStyleNames="btn-lg">Search</b:Button>
				    </b:InputGroupButton>
				</b:InputGroup>
			</b:Row>
			<b:Row>
	       		<g:FlowPanel ui:field="mainContainer" />
	       	</b:Row>
	       	<b:Row>
				<g:SimplePanel ui:field="paginationPanel"/>
			</b:Row>
			<b:Row>
				<b:Container addStyleNames="clear"></b:Container>
			</b:Row>
        </b:Container>
        <g:SimplePanel ui:field="footer" />
	</g:HTMLPanel>
</ui:UiBinder> 