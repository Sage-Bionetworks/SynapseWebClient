<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view">

	<g:HTMLPanel>
	    <div class="color-line"></div>
		<g:SimplePanel ui:field="header" />
		<div class="separator separator-header-page"></div>
	    <div class="row margin-top-left-10 margin-right-15">
	        <div class="row">
	            <g:SimplePanel ui:field="mediaObjectContainer" styleName="col-md-9"/>
	    		<g:FlowPanel ui:field="commandsContainer" styleName="col-md-3"/>
	        </div>
	    	<div class="row">
		    	<g:FlowPanel ui:field="mainContainer" styleName="col-md-12"/>
		   	</div>
	    	
		    <div class="clear"></div>
        </div>
		<g:SimplePanel ui:field="footer" />

	</g:HTMLPanel>
</ui:UiBinder> 