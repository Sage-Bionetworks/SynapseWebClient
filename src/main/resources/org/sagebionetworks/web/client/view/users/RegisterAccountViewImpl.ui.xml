<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<g:HTMLPanel>
		<div class="container margin-top-15 min-height-400">
			<b:Heading size="H2" text="Start using Synapse." addStyleNames="text-align-center margin-top-20"/>
			<b:Heading size="H4" subText="It's free." addStyleNames="text-align-center margin-top-20"/>
			<b:Container fluid="true">
				<b:Row>
					<b:Column size="SM_3"></b:Column>
					<b:Column size="SM_6">
						<bh:Div ui:field="registerWidgetContainer" addStyleNames="margin-top-20"/>
					</b:Column>
					<b:Column size="SM_3"></b:Column>
				</b:Row>
			</b:Container>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 