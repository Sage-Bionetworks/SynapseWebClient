<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<g:HTMLPanel>
		<div class="container margin-top-15 min-height-400">
			<bh:Div width="365px" addStyleNames="center-in-div">
				<b:Heading size="H2" text="Create Synapse Account" addStyleNames="text-align-center margin-top-20"/>
				<b:Heading size="H4" subText="Please enter your email address and we will send you the instructions on how to complete the registration process." addStyleNames="flexcontainer-justify-center margin-top-20"/>
				<bh:Div ui:field="registerWidgetContainer" addStyleNames="margin-top-20"/>
				<b:Heading size="H5" addStyleNames="text-align-center margin-top-20">
					<b:Anchor text="&lt; Back to Sign In" addStyleNames="synapse-green" href="#!LoginPlace:0"/>
				</b:Heading>
			</bh:Div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 