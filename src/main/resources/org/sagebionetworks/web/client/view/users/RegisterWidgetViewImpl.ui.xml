<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:s="urn:import:org.sagebionetworks.web.client.view" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div addStyleNames="synapse-blue-bg padding-10">
		<b:Heading size="H3" addStyleNames="color-white margin-top-5" text="NEW TO SYNAPSE? REGISTER NOW"></b:Heading>
		<g:TextBox ui:field="emailAddressField" width="100%" addStyleNames="margin-top-20" />
		<bh:Div height="88px">
			<b:Button ui:field="registerBtn" pull="RIGHT" size="LARGE" addStyleNames="margin-top-40">Register</b:Button>
		</bh:Div>
	</bh:Div>
</ui:UiBinder> 