<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	<b:TabPanel>
		<b:NavTabs>
			<b:TabListItem active="true" dataTarget="#commandLineTab" text="Command Line" />
			<b:TabListItem dataTarget="#javaTab" text="Java" />
			<b:TabListItem dataTarget="#pythonTab" text="Python" />
			<b:TabListItem dataTarget="#rTab" text="R" />
		</b:NavTabs>
		<b:TabContent>
			<b:TabPane active="true" b:id="commandLineTab">
				<g:HTMLPanel>
					<pre>
						<b:Code ui:field="commandLineCode" addStyleNames="powershell"/>
					</pre>
				</g:HTMLPanel>
			</b:TabPane>
			<b:TabPane b:id="javaTab">
				<g:HTMLPanel>
					<pre>
						<b:Code ui:field="javaCode" addStyleNames="java"/>
					</pre>
				</g:HTMLPanel>
			</b:TabPane>
			<b:TabPane b:id="pythonTab">
				<g:HTMLPanel>
					<pre>
						<b:Code ui:field="pythonCode" addStyleNames="python"/>
					</pre>
				</g:HTMLPanel>
			</b:TabPane>
			<b:TabPane b:id="rTab">
				<g:HTMLPanel>
					<pre>
						<b:Code ui:field="rCode" addStyleNames="r"/>
					</pre>
				</g:HTMLPanel>
			</b:TabPane>
		</b:TabContent>
	</b:TabPanel>

</ui:UiBinder>