<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
>
  <bh:Div ui:field="container">
    <b:TextArea
      addStyleNames="hide-resize"
      ui:field="commaSeparatedTextBox"
      placeholder="Paste comma or tab delimited values here."
      visibleLines="5"
    />
    <b:html.Div
      addStyleNames="flexcontainer-row flexcontainer-justify-flex-end"
    >
      <b:Button ui:field="cancelButton" type="LINK">CANCEL</b:Button>
      <b:Button ui:field="addButton" type="LINK">ADD</b:Button>
    </b:html.Div>
  </bh:Div>
</ui:UiBinder>
