<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div>
		<b:FieldSet>
			<b:Legend text="Please tell us about your project"></b:Legend>
		</b:FieldSet>
		<bh:Div>
			<b:FormGroup>
				<b:FormLabel>Project Lead</b:FormLabel>
				<b:TextBox ui:field="projectLeadField" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Institution</b:FormLabel>
				<b:TextBox ui:field="institutionField" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Intended Data Use Statement</b:FormLabel>
				<bh:Italic ui:field="visibleToPublicNote" visible="false"
					text=" - this will be visible to the public" />
				<b:TextArea ui:field="intendedDataUseStatementField"
					visibleLines="10" />
			</b:FormGroup>
		</bh:Div>
	</bh:Div>
</ui:UiBinder>