<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	 <t:Table width="100%" addStyleNames="margin-left-10 margin-right-10 table-layout-fixed">
	 	<bh:Div ui:field="synAlertContainer"/>
	 	<bh:Span ui:field="promptModalContainer"/>
		<t:TableRow>
			<t:TableData width="10%" >
				<g:Label ui:field="createdOnField"/>
			</t:TableData>
			<t:TableData width="10%" addStyleNames="hidden-xs">
				<g:Label ui:field="stateField"/>
			</t:TableData>
			<t:TableData width="10%">
				<g:Label ui:field="institutionField"/>
			</t:TableData>
			<t:TableData width="10%" addStyleNames="hidden-xs">
				<g:Label ui:field="projectLeadField"/>
			</t:TableData>
			<t:TableData width="15%" addStyleNames="hidden-xs">
				<g:TextBox ui:field="intendedDataUseField" width="100%" addStyleNames="border-none noBackground font-size-14" readOnly="true" />
			</t:TableData>
			<t:TableData width="15%" addStyleNames="hidden-xs">
				<bh:Div ui:field="accessorsContainer" />
			</t:TableData>
			<t:TableData ui:field="ducColumn" width="10%" addStyleNames="hidden-xs">
				<bh:Div ui:field="ducContainer" />
			</t:TableData>
			<t:TableData ui:field="irbColumn" width="10%" addStyleNames="hidden-xs">
				<bh:Div ui:field="irbContainer" />
			</t:TableData>
			<t:TableData ui:field="otherAttachmentsColumn" width="10%">
				<bh:Div ui:field="otherAttachmentsContainer" />
			</t:TableData>
			<t:TableData ui:field="isRenewalColumn" width="5%">
				<b:CheckBox ui:field="renewalCheckbox"/>
			</t:TableData>
			<t:TableData ui:field="publicationsColumn" width="10%">
				<g:TextBox ui:field="publicationsField" width="100%" addStyleNames="border-none noBackground font-size-14" readOnly="true" />
			</t:TableData>
			<t:TableData ui:field="summaryOfUseColumn" width="10%">
				<g:TextBox ui:field="summaryOfUseField" width="100%" addStyleNames="border-none noBackground font-size-14" readOnly="true" />
			</t:TableData>
			<t:TableData width="10%">
				<b:Button ui:field="rejectButton" type="WARNING" size="EXTRA_SMALL" addStyleNames="margin-top-10 margin-right-2" visible="false" text="Reject" />
				<b:Button ui:field="approveButton" type="SUCCESS" size="EXTRA_SMALL" addStyleNames="margin-top-10 margin-right-2" visible="false" text="Approve" />
			</t:TableData>
		</t:TableRow>
	</t:Table>
</ui:UiBinder>
