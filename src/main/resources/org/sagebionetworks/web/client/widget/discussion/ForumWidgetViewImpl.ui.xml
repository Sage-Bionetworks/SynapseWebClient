<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.entity.menu.v2">
	<bh:Div>
		<b:Row>
			<b:Column size="XS_12">
				<b:Button ui:field="newThreadButton" type="PRIMARY"
					pull="RIGHT">New Thread</b:Button>
				<bh:Span ui:field="subscribeButtonContainer"
					addStyleNames="right margin-right-10" />
				<bh:Span ui:field="subscribersContainer"
					addStyleNames="right margin-right-10 movedown-7" />
			</b:Column>
		</b:Row>
		<bh:Div ui:field="deleteThreadsArea"
			addStyleNames="highlight-box padding-top-0-imp margin-bottom-20"
			visible="false">
			<b:Row addStyleNames="margin-top-10">
				<b:Column size="XS_12">
					<bh:Span addStyleNames="highlight-title">Deleted Threads</bh:Span>
				</b:Column>
			</b:Row>
			<b:Row>
				<b:Column size="XS_12">
					<g:SimplePanel addStyleNames="margin-top-10"
						ui:field="deletedThreadListContainer" visible="false" />
				</b:Column>
			</b:Row>
		</bh:Div>
		<bh:Div ui:field="mainContainer"
			addStyleNames="padding-top-0-imp margin-bottom-20 ">
			<b:Row>
				<b:Column size="XS_12">
					<b:Button ui:field="showAllThreadsButton" type="LINK"
						icon="ARROW_LEFT" addStyleNames="margin-top-10"
						visible="false">Show All Threads</b:Button>
				</b:Column>
			</b:Row>
			<b:Row>
				<b:Column size="XS_12">
					<g:SimplePanel addStyleNames="margin-top-10 light-border"
						ui:field="threadListContainer" />
				</b:Column>
				<b:Column size="XS_12">
					<bh:Div ui:field="singleThreadAndSortContainer" addStyleNames="light-border" paddingTop="15" paddingRight="30" paddingBottom="15" paddingLeft="30">
						<b:ButtonGroup ui:field="repliesSortButtonGroup" addStyleNames="flexcontainer-row flexcontainer-justify-center" visible="false">
							<b:Button ui:field="sortRepliesAscendingButton"
								text="Date Posted" addStyleNames="margin-top-10" />
							<b:Button ui:field="sortRepliesDescendingButton"
								text="Most Recent" addStyleNames="margin-top-10" />
						</b:ButtonGroup>
						<g:SimplePanel addStyleNames="margin-top-10"
							ui:field="singleThreadContainer" />
					</bh:Div>
				</b:Column>
				<b:Column size="XS_12">
					<g:SimplePanel addStyleNames="margin-top-10 light-border"
						ui:field="defaultThreadContainer" visible="false" />
				</b:Column>
			</b:Row>
		</bh:Div>
		<g:SimplePanel ui:field="newThreadModalContainer" />
		<bh:Div ui:field="synAlertContainer"></bh:Div>
	</bh:Div>
</ui:UiBinder>
