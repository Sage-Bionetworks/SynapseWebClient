<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<g:FocusPanel ui:field="followersFocusPanel">
			<bh:Span ui:field="followersLink"
				addStyleNames="imageButton link" />
		</g:FocusPanel>
		<b:Modal ui:field="modal" closable="true"
			dataBackdrop="STATIC" dataKeyboard="true" title="Followers">
			<b:ModalBody>
				<bh:Div ui:field="userListContainer" />
				<bh:Div ui:field="synAlertContainer" />
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="okButton" type="PRIMARY" text="OK" />
			</b:ModalFooter>
		</b:Modal>
	</bh:Div>

</ui:UiBinder> 