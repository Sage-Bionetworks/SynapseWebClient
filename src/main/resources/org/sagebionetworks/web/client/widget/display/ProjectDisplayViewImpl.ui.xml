<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.entity.menu.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">
	<!-- xmlns:b3="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui" -->
	<g:HTMLPanel>
	    <b:Modal ui:field="modal" closable="true"
			dataBackdrop="STATIC" dataKeyboard="false" size="LARGE">
			<b:ModalHeader>
				<b:Heading size="H4" text="Project Display Update" addStyleNames="displayInline" />
			</b:ModalHeader>
			<b:ModalBody>
			     <b:FormGroup>
			     	<b:Radio ui:field="wikiButton" name="displayRadio" text="Wiki" addStyleNames="font-size-18"/>
			     	<b:Radio ui:field="filesButton" name="displayRadio" text="Files" addStyleNames="font-size-18" />
			     	<b:Radio ui:field="tablesButton" name="displayRadio" text="Tables" addStyleNames="font-size-18" />
			     	<b:Radio ui:field="challengeButton" name="displayRadio" text="Challenge" addStyleNames="font-size-18" />
			     	<b:Radio ui:field="discussionButton" name="displayRadio" text="Discussion" addStyleNames="font-size-18" />
			     	<!-- <b3:ToggleSwitch ui:field="dockerButton" name="displayRadio" text="Docker" addStyleNames="font-size-18" /> -->
			    </b:FormGroup>
			   <g:SimplePanel ui:field="synAlertPanel"/>
			</b:ModalBody>
			<b:ModalFooter>
			    <b:Button ui:field="saveButton" text="Save" type="PRIMARY"/>
			    <b:Button text="Cancel" ui:field="cancelButton" addStyleNames="right margin-right-5"/>
			</b:ModalFooter>
		</b:Modal>
	</g:HTMLPanel>
</ui:UiBinder>  
