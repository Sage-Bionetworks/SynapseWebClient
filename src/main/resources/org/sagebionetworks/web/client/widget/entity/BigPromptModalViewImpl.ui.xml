<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<b:Modal ui:field="modal" title="Title goes here"
		closable="true" dataBackdrop="STATIC" dataKeyboard="true">
		<b:ModalBody>
			<bh:Div>
				<b:FormGroup>
					<b:FormLabel ui:field="nameLabel" for="formName">Label goes here</b:FormLabel>
					<b:TextArea b:id="formName" ui:field="nameField"
						visibleLines="5" />
				</b:FormGroup>
			</bh:Div>
			<b:Alert ui:field="alert" type="DANGER" visible="false"></b:Alert>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="defaultButton" type="LINK" text="Cancel"></b:Button>
			<b:Button ui:field="primaryButton" type="PRIMARY"
				dataLoadingText="Saving..." text="Save"></b:Button>
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>