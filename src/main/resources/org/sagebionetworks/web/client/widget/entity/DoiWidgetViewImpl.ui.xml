<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2">
	<bh:Span> 
	    <b:Alert ui:field="errorCreatingDoi" addStyleNames="boldText" visible="false">
	        <bh:Strong text="Error creating DOI"/>
	    </b:Alert>
	    <bh:Span ui:field="doiLabel" addStyleNames="boldText margin-right-5" text="DOI: " visible="false"/>
	    <g:TextBox ui:field="doi" width="240px" addStyleNames="border-none noBackground padding-0" readOnly="true" />
	    <g:SimplePanel ui:field="doiProcessing" addStyleNames="inline-block padding-right-5">
	        <bh:Text text="DOI processing"/>
	    </g:SimplePanel>
	</bh:Span>
</ui:UiBinder>