<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<!-- Dialog for creating a new table -->
	<b:Modal ui:field="modal" title="Edit File Metadata"
		closable="true" dataBackdrop="STATIC" dataKeyboard="true">
		<b:ModalBody>
			<bh:Div>
				<b:FormGroup>
					<b:FormLabel>Synapse name</b:FormLabel>
					<b:TextBox ui:field="entityNameField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel>Download file as</b:FormLabel>
					<b:TextBox ui:field="fileNameField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel>Content type</b:FormLabel>
					<b:TextBox ui:field="contentTypeField" />
				</b:FormGroup>

			</bh:Div>
			<b:Alert ui:field="alert" type="DANGER" visible="false"></b:Alert>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="defaultButton" type="LINK"
				dataDismiss="MODAL">Cancel</b:Button>
			<b:Button ui:field="primaryButton" type="PRIMARY"
				dataLoadingText="Saving...">Save</b:Button>
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>