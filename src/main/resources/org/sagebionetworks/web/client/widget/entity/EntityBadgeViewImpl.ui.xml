<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
>
  <t:Table width="100%" height="36px" addStyleNames="entityBadge">
    <t:TableRow>
      <t:TableData
        addStyleNames="min-width-30 text-align-center overflow-x-hidden"
        width="30px"
      >
        <g:FocusPanel ui:field="entityTypeIconContainer">
          <w:EntityTypeIconImpl ui:field="entityTypeIcon" type="file" />
        </g:FocusPanel>
      </t:TableData>
      <t:TableData ui:field="entityTableData" paddingTop="6">
        <g:FlowPanel
          ui:field="entityContainer"
          addStyleNames="displayInline margin-left-5 margin-right-5"
        />
      </t:TableData>
      <t:TableData
        addStyleNames="overflow-x-hidden min-width-140"
        width="140px"
      >
        <w:ReactComponent ui:field="iconsContainer" />
      </t:TableData>
      <t:TableData
        ui:field="sizeTableData"
        addStyleNames="hidden-xs overflow-x-hidden text-align-right min-width-140"
        width="140px"
      >
        <g:Label ui:field="sizeField" addStyleNames="margin-right-30" />
      </t:TableData>
      <t:TableData
        ui:field="modifiedOnTableData"
        addStyleNames="visible-md visible-lg overflow-x-hidden min-width-165"
        width="165px"
      >
        <g:Label ui:field="modifiedOnField" />
      </t:TableData>
      <t:TableData
        ui:field="createdOnTableData"
        addStyleNames="visible-xl overflow-x-hidden min-width-165"
        width="165px"
      >
        <g:Label ui:field="createdOnField" />
      </t:TableData>
      <t:TableData
        ui:field="idTableData"
        addStyleNames="visible-lg overflow-x-hidden min-width-150"
        width="150px"
      >
        <g:TextBox
          ui:field="idField"
          addStyleNames="border-none noBackground"
          readOnly="true"
          width="100%"
        />
      </t:TableData>
      <t:TableData
        ui:field="md5TableData"
        addStyleNames="visible-xl overflow-x-hidden min-width-165"
        width="165px"
      >
        <g:TextBox
          ui:field="md5Field"
          addStyleNames="border-none noBackground overflow-x-hidden"
          readOnly="true"
          width="100%"
        />
      </t:TableData>
      <t:TableData
        ui:field="modifiedByTableData"
        addStyleNames="visible-xl overflow-x-hidden min-width-165 max-width-165"
        width="165px"
      >
        <g:SimplePanel ui:field="modifiedByField" />
      </t:TableData>
      <t:TableData
        ui:field="downloadTableData"
        addStyleNames="overflow-x-hidden text-align-center hidden-xs min-width-70"
        width="70px"
      >
        <b:Anchor
          ui:field="addToDownloadListLink"
          addStyleNames="textDecorationNone"
          visible="false"
        >
          <i class="fa syn-download moveright-5 movedown-1" />
          +
        </b:Anchor>
      </t:TableData>
    </t:TableRow>
  </t:Table>
</ui:UiBinder>
