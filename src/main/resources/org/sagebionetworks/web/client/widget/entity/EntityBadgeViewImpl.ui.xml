<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	<t:Table width="100%" height="36px" addStyleNames="entityBadge">
		<t:TableRow>
			<t:TableData
				addStyleNames="min-width-25 overflow-x-hidden">
				<g:FocusPanel ui:field="iconContainer">
					<b:Icon ui:field="icon" type="FILE" />
				</g:FocusPanel>
			</t:TableData>
			<t:TableData ui:field="entityTableData">
				<g:FlowPanel ui:field="entityContainer"
					addStyleNames="displayInline margin-right-5" />
			</t:TableData>
			<t:TableData
				addStyleNames="overflow-x-hidden min-width-140">
				<bh:Div ui:field="iconsContainer" />
			</t:TableData>
			<t:TableData ui:field="sizeTableData"
				addStyleNames="hidden-xs overflow-x-hidden text-align-right min-width-140">
				<g:Label ui:field="sizeField"
					addStyleNames="margin-right-30" />
			</t:TableData>
			<t:TableData ui:field="modifiedOnTableData"
				addStyleNames="visible-md visible-lg overflow-x-hidden min-width-165">
				<g:Label ui:field="modifiedOnField" />
			</t:TableData>
			<t:TableData ui:field="createdOnTableData"
				addStyleNames="visible-xl overflow-x-hidden min-width-165">
				<g:Label ui:field="createdOnField" />
			</t:TableData>
			<t:TableData ui:field="idTableData"
				addStyleNames="visible-lg overflow-x-hidden min-width-150">
				<g:TextBox ui:field="idField"
					addStyleNames="border-none noBackground font-size-14"
					readOnly="true" />
			</t:TableData>
			<t:TableData ui:field="md5TableData"
				addStyleNames="visible-xl overflow-x-hidden min-width-150">
				<g:TextBox ui:field="md5Field"
					addStyleNames="border-none noBackground font-size-14 overflow-x-hidden"
					readOnly="true" />
			</t:TableData>
			<t:TableData ui:field="modifiedByTableData"
				addStyleNames="visible-xl overflow-x-hidden min-width-310 max-width-310">
				<g:SimplePanel ui:field="modifiedByField" />
			</t:TableData>
			<t:TableData ui:field="downloadTableData"
				addStyleNames="overflow-x-hidden text-align-center hidden-xs min-width-70">
				<b:Anchor ui:field="addToDownloadListLink"
					addStyleNames="textDecorationNone" visible="false">
					<i class="fa syn-download moveright-5 movedown-1"></i>
					+
				</b:Anchor>
			</t:TableData>

		</t:TableRow>
	</t:Table>
</ui:UiBinder>
