<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
  <ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
  	<bh:Div addStyleNames="border-top-1">
  		<g:HTMLPanel ui:field="loadingUI">
			<g:Image styleName="displayInline" resource='{sageImageBundle.loading16}' />
			&nbsp;Loading...
		</g:HTMLPanel>
	  	<t:Table width="100%" height="36px">
			<t:TableRow ui:field="row" visible="false">
				<t:TableData width="35px" ui:field="selectTableData">
					<b:CheckBox ui:field="select" />
				</t:TableData>
				<t:TableData ui:field="synAlertTableData">
					<bh:Span ui:field="synAlertContainer" />
				</t:TableData>
				<t:TableData width="20px" ui:field="iconTableData">
					<b:Icon ui:field="icon" type="FILE"/>
				</t:TableData>
				<t:TableData addStyleNames="maxWidth50px overflow-x-hidden " ui:field="linkTableData">
					<bh:Div addStyleNames="margin-right-5"> 
						<g:Anchor ui:field="entityLink" styleName="displayInline pull-left link"/>
						<bh:Span ui:field="fileDownloadButtonContainer" addStyleNames="right margin-right-10"/>
					</bh:Div>
				</t:TableData>
				<t:TableData width="60px" addStyleNames="overflow-x-hidden" ui:field="versionTableData">
					<g:Label ui:field="versionField"/>
				</t:TableData>
				<t:TableData width="165px" ui:field="descriptionTableData" addStyleNames="overflow-x-hidden" visible="false">
					<g:Label ui:field="descriptionField"/>
				</t:TableData>
				<t:TableData width="165px" addStyleNames="visible-md visible-lg overflow-x-hidden" ui:field="createdOnTableData">
					<g:Label ui:field="createdOnField"/>
				</t:TableData>
				<t:TableData width="310px" addStyleNames="visible-xl overflow-x-hidden" ui:field="createdByTableData">
					<g:SimplePanel ui:field="createdByField"/>
				</t:TableData>
				<t:TableData width="165px" ui:field="noteTableData" addStyleNames="visible-md visible-lg overflow-x-hidden">
					<g:Label ui:field="noteField"/>
				</t:TableData>
			</t:TableRow>
		</t:Table>  
  	</bh:Div>
	  
</ui:UiBinder>
