<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />

	<g:HTMLPanel styleName="margin-left-15 margin-right-15">											
				<g:SimplePanel styleName="margin-top-5" ui:field="projectTitleContainer"/>
								
   				<div class="row">
				    <g:SimplePanel ui:field="topFullWidthPanel"/>
			    </div>
				    
				<div class="row margin-top-15" ui:field="navtabContainer">
				    <ul class="nav nav-tabs entity-tabs">				        
					    <li class="entity-tab active margin-left-15" ui:field="wikiListItem"><g:Anchor styleName="text-align-center no-background" ui:field="wikiLink"/></li>
					    <li class="entity-tab" ui:field="filesListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="fileLink"/></li>
					    <li class="entity-tab" ui:field="adminListItem"><g:Anchor styleName="text-align-center link no-background" ui:field="adminLink"/></li>
				    </ul>
				</div>
				
				<div class="row">
					<div class="col-md-8"><g:SimplePanel ui:field="colLeftPanel" /></div>
					<div class="col-md-4" style="float:right; !important"><g:SimplePanel ui:field="colRightPanel" /></div>
				</div>
											
				<g:SimplePanel ui:field="fullWidthPanel"/>
	</g:HTMLPanel>
</ui:UiBinder>

