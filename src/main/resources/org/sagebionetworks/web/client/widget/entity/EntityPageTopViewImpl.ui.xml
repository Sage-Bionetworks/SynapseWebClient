<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">

	<g:HTMLPanel>
		<!--Project level information -->
		<bh:Div addStyleNames="min-height-48 lightGreyBackground padding-left-15 padding-right-15">
			<b:Row addStyleNames="padding-top-15 projectMetadata">
				<b:Column size="XS_12">
					<bh:Span ui:field="projectActionMenuContainer"
						addStyleNames="right" />
					<bh:Div addStyleNames="margin-left-15 margin-right-15">
						<g:SimplePanel ui:field="projectMetadataContainer" />
					</bh:Div>
					<w:LoadingSpinner size="40px" ui:field="loadingUI"
						addStyleNames="center-block center" />
				</b:Column>
			</b:Row>
		</bh:Div>
		<bh:Div ui:field="tabsUI"/>
	</g:HTMLPanel>
</ui:UiBinder>

