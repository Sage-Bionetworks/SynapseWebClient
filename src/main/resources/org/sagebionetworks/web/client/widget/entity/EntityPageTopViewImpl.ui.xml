<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />

	<g:HTMLPanel>
				<div class="span-24 notopmargin" style="margin-top: 5px !important">
					<div class="span-14 notopmargin" style="white-space: normal;">
						<g:SimplePanel ui:field="breadcrumbsPanel"/>
					</div>
					<div class="span-10 last right notopmargin">
						<g:SimplePanel ui:field="actionMenuPanel" />
					</div>
				</div>

				<div class="span-24" ui:field="navtabContainer">
				    <ul class="nav nav-tabs">
					    <li class="active" ui:field="wikiListItem"><g:Anchor ui:field="wikiLink" text="Wiki"/></li>
					    <li ui:field="filesListItem"><g:Anchor ui:field="fileLink" text="Files"/></li>
					    <li ui:field="adminListItem"><g:Anchor ui:field="adminLink" text="Admin"/></li>
				    </ul>
				</div>
				<div class="span-24" style="margin-top: 5px">
					<div class="span-17 notopmargin left"><g:SimplePanel ui:field="colLeftPanel" /></div>
					<div class="span-7 notopmargin right last" style="float:right; !important"><g:SimplePanel ui:field="colRightPanel" /></div>
					<div class="span-24 notopmargin left last"><g:SimplePanel ui:field="fullWidthPanel" /></div>
				</div>								
	</g:HTMLPanel>
</ui:UiBinder>

