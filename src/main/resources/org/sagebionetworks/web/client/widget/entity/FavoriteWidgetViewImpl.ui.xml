<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  >
  	<ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
  	<bh:Span addStyleNames="margin-left-2">
		<b:Tooltip ui:field="tip" title="Click the star to add this to your favorites!" trigger="HOVER" placement="RIGHT">
			<b:Anchor ui:field="favoriteIcon" icon="STAR"/>
		</b:Tooltip>
		<g:Image ui:field="loadingUI" styleName="displayInline" resource='{sageImageBundle.loading16}' />
	</bh:Span>
</ui:UiBinder>
