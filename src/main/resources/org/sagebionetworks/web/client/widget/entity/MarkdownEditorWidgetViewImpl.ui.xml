<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	<bh:Div addStyleNames="markdownEditor">
		<bh:Div ui:field="writingUI">
			<bh:Div ui:field="mdCommands" styleName="view md-command-bar">
				<b:Button ui:field="markdownPreviewButton"
					size="EXTRA_SMALL" height="22px" text="Preview" type="DEFAULT"
					addStyleNames="margin-left-2 text-primary" />
				<bh:Span
					addStyleNames="border-left-1 margin-left-5 margin-right-5" />
				<b:Tooltip placement="BOTTOM"
					title="Edit an existing Synapse Widget (must place cursor on widget markdown to enable)">
					<b:Button ui:field="editWidgetButton" icon="EDIT"
						size="EXTRA_SMALL" height="22px" text="Edit Synapse Widget" />
				</b:Tooltip>
				<b:ButtonGroup>
					<b:Button dataToggle="DROPDOWN" size="EXTRA_SMALL"
						icon="PLUS_CIRCLE">Insert</b:Button>
					<b:DropDownMenu>
						<b:AnchorListItem ui:field="attachmentLink"
							text="Attachment" />
						<b:AnchorListItem ui:field="buttonLink"
							text="Button Link" />
						<b:AnchorListItem ui:field="detailsSummaryLink"
							text="Collapsible Section (Details/Summary)" />
						<b:AnchorListItem ui:field="entityListLink"
							text="Entity List" />
						<b:AnchorListItem ui:field="previewLink"
							text="File Preview" />
						<b:AnchorListItem ui:field="genomeBrowserLink"
							text="Genome Browser" />
						<b:AnchorListItem ui:field="imageLinkMenuItem"
							text="Image" visible="false" />
						<b:AnchorListItem ui:field="imageLink"
							text="Image" />
						<b:AnchorListItem ui:field="joinTeamLink"
							text="Join Team Button" />
						<b:AnchorListItem ui:field="leaderboardLink"
							text="Leaderboard" />
						<b:AnchorListItem ui:field="linkLink" text="Link" />
						<b:AnchorListItem ui:field="provenanceGraphLink"
							text="Provenance Graph" />
						<b:AnchorListItem ui:field="referenceLink"
							text="Reference" />
						<b:AnchorListItem ui:field="graphLink"
							icon="BAR_CHART" text="Simple Plot" iconPosition="RIGHT" />
						<b:AnchorListItem
							ui:field="submitToEvaluationLink" text="Submit to Evaluation" />
						<b:AnchorListItem ui:field="tableLink"
							text="Table: Paste tabular data" />
						<b:AnchorListItem ui:field="synapseTableLink"
							text="Table: Query on a Synapse Table/View" />
						<b:AnchorListItem ui:field="queryLink"
							text="Table: Query on Files/Folders" visible="false" />
						<b:AnchorListItem ui:field="tableOfContentsLink"
							text="Table of Contents" />
						<b:AnchorListItem ui:field="teamLink"
							text="Team Badge" />
						<b:AnchorListItem ui:field="teamMemberCountLink"
							text="Team Member Count" />
						<b:AnchorListItem ui:field="teamMembersLink"
							text="Team Members List" />
						<b:AnchorListItem ui:field="userLink" text="User" />
						<b:AnchorListItem ui:field="videoLink"
							icon="VIDEO_CAMERA" iconPosition="RIGHT" text="Video" />
					</b:DropDownMenu>
				</b:ButtonGroup>
				<b:ButtonGroup>
					<b:Tooltip placement="BOTTOM"
						title="WARNING! These are unreleased widgets, whose syntax may change in the future.">
						<b:Button dataToggle="DROPDOWN" visible="false"
							ui:field="alphaInsertButton" size="EXTRA_SMALL">@ Insert</b:Button>
					</b:Tooltip>
					<b:DropDownMenu>
						<b:AnchorListItem
							ui:field="challengeParticipantsLink"
							text="Challenge Participants List" />
						<b:AnchorListItem ui:field="challengeTeamsLink"
							text="Challenge Teams List" />
						<b:AnchorListItem ui:field="cytoscapeJsLink"
							text="Cytoscape JS Graph" />
						<b:AnchorListItem ui:field="externalWebsiteLink"
							text="External Website" />
						<b:AnchorListItem
							ui:field="registerChallengeTeamLink"
							text="Register Challenge Team Button" />
						<b:AnchorListItem ui:field="supertableLink"
							text="Synapse API SuperTable" />
						<b:AnchorListItem ui:field="synapseFormLink"
							text="Synapse Form" />
						<b:AnchorListItem ui:field="tutorialWizardLink"
							text="Tutorial Wizard" />
						<b:AnchorListItem ui:field="wikifilesPreviewLink"
							text="Wiki Files Preview" />
					</b:DropDownMenu>
				</b:ButtonGroup>

				<b:Tooltip placement="BOTTOM" title="Bold">
					<b:Button ui:field="boldButton" icon="BOLD"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Italic">
					<b:Button ui:field="italicButton" icon="ITALIC"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Strike-through">
					<b:Button ui:field="strikeButton" icon="STRIKETHROUGH"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>

				<b:Tooltip placement="BOTTOM"
					title="Optionally specify the language for syntax highlighting.">
					<b:Button ui:field="codeBlockButton" size="EXTRA_SMALL">Code Block</b:Button>
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="LaTeX math equation.">
					<b:Button ui:field="mathButton" size="EXTRA_SMALL">TeX</b:Button>
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Subscript">
					<b:Button ui:field="subscriptButton" icon="SUBSCRIPT"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Superscript">
					<b:Button ui:field="superscriptButton" icon="SUPERSCRIPT"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:ButtonGroup>
					<b:Button dataToggle="DROPDOWN" size="EXTRA_SMALL">Heading</b:Button>
					<b:DropDownMenu>
						<b:AnchorListItem ui:field="heading1Link"
							text="Heading 1" />
						<b:AnchorListItem ui:field="heading2Link"
							text="Heading 2" />
						<b:AnchorListItem ui:field="heading3Link"
							text="Heading 3" />
						<b:AnchorListItem ui:field="heading4Link"
							text="Heading 4" />
						<b:AnchorListItem ui:field="heading5Link"
							text="Heading 5" />
						<b:AnchorListItem ui:field="heading6Link"
							text="Heading 6" />
					</b:DropDownMenu>
				</b:ButtonGroup>
				<b:Tooltip placement="BOTTOM" title="Insert Attachment">
					<b:Button ui:field="attachmentButton" icon="PAPERCLIP"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Insert Image">
					<b:Button ui:field="imageButton" icon="CAMERA"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Insert Image">
					<b:Button ui:field="imageLinkButton" icon="CAMERA"
						size="EXTRA_SMALL" height="22px" visible="false" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Insert Video">
					<b:Button ui:field="videoButton" icon="VIDEO_CAMERA"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Insert Link">
					<b:Button ui:field="linkButton" icon="LINK"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Tooltip placement="BOTTOM" title="Tag someone">
					<b:Button ui:field="userButton" icon="USER_PLUS"
						size="EXTRA_SMALL" height="22px" />
				</b:Tooltip>
				<b:Button ui:field="formattingGuideButton"
					icon="QUESTION_CIRCLE" size="EXTRA_SMALL" text="Formatting Guide"
					pull="RIGHT" />
			</bh:Div>
			<bh:Div>
				<b:TextArea ui:field="markdownTextArea"
					addStyleNames="hide-resize" />
			</bh:Div>
		</bh:Div>

		<bh:Div ui:field="previewUI" visible="false">
			<bh:Div addStyleNames="margin-bottom-5">
				<b:Button ui:field="writeMarkdownButton" text="Edit"
					type="PRIMARY" icon="ARROW_LEFT" size="EXTRA_SMALL"
					addStyleNames="margin-left-2" />
			</bh:Div>
			<bh:Div addStyleNames="light-border">
				<g:SimplePanel ui:field="previewHtmlContainer"
					addStyleNames="entity-description-preview-wrapper" />
			</bh:Div>
		</bh:Div>
		<bh:Div ui:field="selectTeamModalContainer" />
	</bh:Div>
</ui:UiBinder>