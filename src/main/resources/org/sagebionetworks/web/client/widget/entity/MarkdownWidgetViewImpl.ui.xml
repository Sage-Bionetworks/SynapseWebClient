<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2">
	<g:FlowPanel>
	    <g:FlowPanel ui:field="wikiSubpagesPanel"/>
	    <g:HTMLPanel ui:field="contentPanel" addStyleNames="margin-right-40 margin-left-10"/>
	    <g:SimplePanel ui:field="synAlertPanel"/>
	    <g:HTMLPanel ui:field="emptyPanel" visible="false">
	    	There is no content. Please click Edit Wiki from the Tools menu.
	    </g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder>