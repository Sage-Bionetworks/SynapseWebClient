<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div addStyleNames="light-border-bottom" marginBottom="9"
		paddingBottom="7">
		<b:Icon ui:field="icon" type="LIST_ALT"
			addStyleNames="displayInline margin-right-5 lightGreyText" />
		<b:Tooltip isHtml="true" placement="RIGHT"
			ui:field="tooltip" trigger="HOVER">
			<g:Anchor ui:field="anchor"
				addStyleNames="displayInline font-size-1-1em" />
		</b:Tooltip>
		<bh:Span ui:field="favoritesWidgetContainer" />
		<bh:Span ui:field="additionalTextUI" width="140px"
			addStyleNames="right hidden-xs">
			<bh:Span ui:field="additionalText" />
		</bh:Span>
	</bh:Div>
</ui:UiBinder>
