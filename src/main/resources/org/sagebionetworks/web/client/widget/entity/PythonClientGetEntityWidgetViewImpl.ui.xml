<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<g:HTMLPanel>
	    <b:Heading size="H5" text="Download this Entity"/>
		<g:HTMLPanel addStyleNames="codeContent">
	 	    <bh:Text text="import synapseclient"/>
	 	    <br/>
	 	    <br/>
	 	    <bh:Text text="syn = synapseclient.Synapse()"/>
	 	    <br/>
	 	    <bh:Text text="syn.login('synapse_username','password')"/>
	 	    <br/>
	 	    <br/>
	 	    <bh:Text text="# Obtain a pointer and download the data"/>
	 	    <br/>
		  	<bh:Text ui:field="download"/>
		  	<br/>
		  	<br/>
		  	<bh:Text text="# Get the path to the local copy of the data file "/>
		  	<br/>
		  	<bh:Text ui:field="getPath"/>
		  	<br/>
 		</g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>