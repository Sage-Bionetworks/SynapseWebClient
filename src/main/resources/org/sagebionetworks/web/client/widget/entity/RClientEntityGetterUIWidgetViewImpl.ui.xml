<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:e="urn:import:org.sagebionetworks.web.client.widget.entity">
  	<ui:with field='iconsImageBundle' type='org.sagebionetworks.web.client.IconsImageBundle'/>
  	<ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
  
	<g:HTMLPanel>
	    <b:Heading size="H5" text="Download this Entity"/>
		<pre class="codeContent">library(synapseClient)
synapseLogin('username','password')

# Obtain a pointer and download the data

<span ui:field="synid1"/> &#60;- synGet(id='<span ui:field="synid2"/>')

# Load the data
<span ui:field="synid3"/> &#60;- synGet(id='<span ui:field="synid4"/>', load=T)</pre>
	</g:HTMLPanel>
</ui:UiBinder>