<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:e="urn:import:org.sagebionetworks.web.client.widget.entity">
  	<ui:with field='iconsImageBundle' type='org.sagebionetworks.web.client.IconsImageBundle'/>
  	<ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
  
	<g:HTMLPanel>
	    <b:Heading size="H5" text="Download this Entity"/>
		<g:HTMLPanel addStyleNames="codeContent">
	 	    <bh:Text text="library(synapseClient)"/>
	 	    <br/>				  	    
	 	    <bh:Text text="synapseLogin('username','password')"/>
	 	    <br/>
	 	    <br/>
	 	    <bh:Text text="# Obtain a pointer and download the data"/>
	 	    <br/>
		  	<bh:Text ui:field="getEntity"/>
		  	<br/>
		  	<br/>
		  	<bh:Text text="# Load the data"/>
		  	<br/>
		  	<bh:Text ui:field="loadEntity"/>
		  	<br/>
 		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>