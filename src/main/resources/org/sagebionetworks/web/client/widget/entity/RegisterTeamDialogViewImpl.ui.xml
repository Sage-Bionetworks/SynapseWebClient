<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:be="urn:import:org.gwtbootstrap3.extras.select.client.ui">
	<b:Modal title="Register Team" closable="false" fade="true" dataBackdrop="STATIC"
		dataKeyboard="false">
		<b:ModalBody>
			<b:FormGroup ui:field="teamSelectionUI">
				<bh:Div ui:field="teamSelectComboUI">
					<b:FormLabel addStyleNames="margin-right-10">Select Team</b:FormLabel>
					<g:ListBox ui:field="teamComboBox" styleName="form-control" />
				</bh:Div>					
				<b:Anchor ui:field="createNewTeamLink">Create a new team</b:Anchor>
				<bh:Paragraph emphasis="WARNING" ui:field="noTeamsFoundUI" visible="false">No teams found</bh:Paragraph>	
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="recruitment">Team recruitment message</b:FormLabel>
				<b:TextBox ui:field="recruitmentMessageField" name="recruitment"
					b:id="recruitment" />
			</b:FormGroup>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="cancelButton" type="DEFAULT" >Cancel</b:Button>
			<b:Button ui:field="okButton" type="PRIMARY">Register</b:Button>
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>