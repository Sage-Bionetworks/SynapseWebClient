<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<b:Modal title="Register Team" closable="false"
		dataBackdrop="STATIC" dataKeyboard="true">
		<b:ModalBody>
			<b:FormGroup ui:field="teamSelectionUI">
				<bh:Div ui:field="teamSelectComboUI">
					<b:FormLabel addStyleNames="margin-right-10">Select Team</b:FormLabel>
					<g:ListBox ui:field="teamComboBox"
						styleName="form-control" />
				</bh:Div>
				<bh:Paragraph ui:field="noTeamsFoundUI" visible="false"
					addStyleNames="font-italic">- No teams found</bh:Paragraph>
				<b:Anchor ui:field="createNewTeamLink"
					text="Create a new team" target="_blank"
					addStyleNames="margin-right-10" />
				<b:Tooltip text="Refresh team list">
					<b:Button ui:field="refreshTeamsButton" icon="REFRESH"
						size="EXTRA_SMALL" />
				</b:Tooltip>
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="recruitment">Team recruitment message</b:FormLabel>
				<b:TextBox ui:field="recruitmentMessageField"
					name="recruitment" b:id="recruitment" />
			</b:FormGroup>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="cancelButton" type="LINK">Cancel</b:Button>
			<b:Button ui:field="okButton" type="PRIMARY">Register</b:Button>
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>