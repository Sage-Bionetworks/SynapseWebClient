<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<b:Modal size="LARGE" closable="true" fade="true" dataBackdrop="STATIC"
		dataKeyboard="false">
		<b:ModalBody>
			<b:Form>
				<b:FieldSet>
					<b:FormGroup>
						<b:FormLabel for="recruitment">Team recruitment message</b:FormLabel>
						<b:TextBox ui:field="recruitmentMessageField" name="recruitment"
							b:id="recruitment" />
					</b:FormGroup>
				</b:FieldSet>
			</b:Form>
			<b:Panel>
				<b:PanelBody>
					<g:SimplePanel ui:field="teamSelectionPanel" />
				</b:PanelBody>
			</b:Panel>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button type="DEFAULT" dataDismiss="MODAL">Cancel</b:Button>
			<b:Button ui:field="okButton" type="PRIMARY">OK</b:Button>
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>