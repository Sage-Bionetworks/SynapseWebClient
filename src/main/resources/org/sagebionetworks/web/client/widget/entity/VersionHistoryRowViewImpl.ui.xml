<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<t:TableRow>
		<t:TableData>
			<g:Anchor ui:field="versionNumberLink" />
			<b:FormControlStatic ui:field="versionNumber"
				visible="false" />
		</t:TableData>
		<t:TableData>
			<g:Anchor ui:field="versionNameLink" />
			<b:FormControlStatic ui:field="versionName"
				addStyleNames="displayInline" visible="false" />
		</t:TableData>
		<t:TableData>
			<g:SimplePanel ui:field="modifiedByContainer" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="modifiedOn" />
		</t:TableData>
		<t:TableData ui:field="sizeTableData">
			<b:FormControlStatic ui:field="size" />
		</t:TableData>
		<t:TableData ui:field="md5TableData">
			<g:SimplePanel ui:field="md5LinkContainer" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="versionComment"
				addStyleNames="displayInline" />
		</t:TableData>
		<t:TableData>
			<bh:Div ui:field="doiWidgetContainer" />
		</t:TableData>
		<t:TableData>
			<b:Tooltip title="Delete version">
				<b:Button icon="TIMES" ui:field="deleteButton" size="SMALL"
					addStyleNames="displayInline margin-left-3" />
			</b:Tooltip>
		</t:TableData>
	</t:TableRow>
</ui:UiBinder>