<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">

	<g:FlowPanel>
		<g:SimplePanel ui:field="synAlertPanel" />
		<g:FlowPanel ui:field="mainPanel">
			<g:FlowPanel ui:field="wikiSubpagesPanel" />
			<g:FlowPanel ui:field="wikiPagePanel">
				<w:LoadingSpinner ui:field="loadingPanel"
					size="31px" />

				<bh:Div ui:field="wikiHeadingContainer"></bh:Div>
				<b:Alert ui:field="diffVersionAlert" visible="false">
					<g:HTMLPanel>
						<bh:Span addStyleNames="margin-left-5"
							text="You are viewing an old version of this page. View the" />
						<b:Anchor ui:field="anchorToCurrentVersion"
							addStyleNames="link" text="current version." />
						<b:Button ui:field="restoreButton"
							addStyleNames="wikiHistoryButton margin-top-10" text="Restore"
							visible="false" />
					</g:HTMLPanel>
				</b:Alert>
				<bh:Italic ui:field="noWikiCanEditMessage"
					text="There is no content. Please click Edit Wiki from the Tools menu."
					visible="false" />
				<bh:Italic ui:field="noWikiCannotEditMessage"
					text="There is no content." visible="false" />
				<g:SimplePanel ui:field="markdownPanel" />
				<g:FlowPanel addStyleNames="margin-top-20 clearleft"
					ui:field="createdModifiedHistoryPanel">
					<bh:Div>
						<bh:Italic text="Wiki created on"
							addStyleNames="margin-right-5" />
						<bh:Italic ui:field="createdOnText" />
						<bh:Italic text="and last modified on"
							addStyleNames="margin-left-5 margin-right-5" />
						<bh:Italic ui:field="modifiedOnText" />
					</bh:Div>

					<b:Button ui:field="wikiCompareButton" iconPosition="LEFT"
						icon="EXCHANGE" text="Wiki Revision Diff Tool" visible="false"
						addStyleNames="margin-top-10 margin-right-10" />
					<b:Button ui:field="wikiHistoryButton" iconPosition="LEFT"
						icon="CARET_SQUARE_O_RIGHT" text="Wiki Revision History"
						addStyleNames="margin-top-10" />
					<b:Collapse ui:field="historyCollapse" toggle="false">
						<g:FlowPanel addStyleNames="margin-top-10"
							ui:field="wikiHistoryPanel" />
					</b:Collapse>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>

</ui:UiBinder>