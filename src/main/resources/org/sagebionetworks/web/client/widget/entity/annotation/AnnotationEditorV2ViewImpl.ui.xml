<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
 	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">
	<g:HTMLPanel>
		<b:Modal ui:field="editModal"
				 closable="true" dataBackdrop="STATIC" dataKeyboard="true" size="LARGE"
				 b:id="editAnnotationsModal">
			<b:ModalHeader>
				<b:Heading size="H4" addStyleNames="displayInline" text="Edit Annotations" />
				<w:HelpWidget
						helpMarkdown="Set metadata annotation fields. Field values are **not** case sensitive."
						href="https://help.synapse.org/docs/Annotations-and-Queries.2011070649.html"
						addStyleNames="margin-left-5" placement="BOTTOM" />
			</b:ModalHeader>

			<b:ModalBody>
				<w:ReactComponentDiv ui:field="reactComponentDiv" />
			</b:ModalBody>
		</b:Modal>
	</g:HTMLPanel>
</ui:UiBinder>