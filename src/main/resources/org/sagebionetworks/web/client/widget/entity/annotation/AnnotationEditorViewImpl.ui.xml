<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	<t:Table addStyleNames="annotations-editor-view" width="100%">
		<t:TableRow>
			<t:TableHeader addStyleNames="min-width-200">
				<b.html:Text>Key</b.html:Text>
			</t:TableHeader>
			<t:TableHeader addStyleNames="min-width-150">
				<b.html:Text>Type</b.html:Text>
			</t:TableHeader>
			<t:TableHeader addStyleNames="min-width-200">
				<b.html:Text>Value</b.html:Text>
			</t:TableHeader>
		</t:TableRow>
		<t:TableRow>
			<t:TableData
				addStyleNames="vertical-align-top min-width-200">
				<!-- Some inherited CSS causes form group to have margin-bottom of 15px, but to ensure that
				addNewAnnotationValueButton can be aligned properly, we must give it a margin of 0px-->
				<b:FormGroup ui:field="formGroup" addStyleNames="margin-bottom-0-imp">
					<b:TextBox ui:field="keyField" />
					<b:HelpBlock ui:field="helpBlock" />
				</b:FormGroup>
			</t:TableData>
			<t:TableData
				addStyleNames="vertical-align-top min-width-150">
				<g:ListBox ui:field="typeComboBox"
					styleName="form-control" />
			</t:TableData>
			<t:TableData
				addStyleNames="vertical-align-top min-width-275">
				<t:Table width="100%" addStyleNames="multi-value-editor-spaced" ui:field="editorsContainer">
				</t:Table>

			</t:TableData>
		</t:TableRow>
	</t:Table>
</ui:UiBinder>