<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:s="urn:import:org.sagebionetworks.web.client.widget.entity.controller">

	<g:HTMLPanel>
		<b:Modal ui:field="editModal" title="Edit Annotations"
			closable="true" dataBackdrop="STATIC" dataKeyboard="true" size="LARGE"
			b:id="editAnnotationsModal">
			<b:ModalBody>
				<g:FlowPanel ui:field="editorsPanel" addStyleNames="max-height-65-vh scroll-y"/>
				<b.html:Div>
					<b:Button ui:field="addAnnotationButton">Add New Key</b:Button>
					<b:Button ui:field="pasteNewValuesButton"
							  type="LINK" addStyleNames="boldText">PASTE NEW VALUES</b:Button>
					<g:FlowPanel addStyleNames="margin-top-10" ui:field="pasteNewValuesPanel" />
				</b.html:Div>

			</b:ModalBody>
			<b:ModalFooter>
				<s:SynapseAlertViewImpl ui:field="alert" />
				<b:Button ui:field="cancelButton" type="LINK"
					dataDismiss="MODAL" addStyleNames="boldText">CANCEL</b:Button>
				<b:Button ui:field="saveButton" type="PRIMARY"
					dataLoadingText="Saving..." addStyleNames="boldText">OK</b:Button>
			</b:ModalFooter>
		</b:Modal>
	</g:HTMLPanel>

</ui:UiBinder>