<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<g:HTMLPanel>
		<b:Modal ui:field="editModal" title="Edit Annotations"
			closable="true" dataBackdrop="STATIC" dataKeyboard="true"
			b:id="editAnnotationsModal">
			<b:ModalBody>
				<g:FlowPanel ui:field="editorsPanel" />
				<b:Button ui:field="addAnnotationButton">Add New Key</b:Button>
				<b:Button ui:field="pasteNewValuesButton"
						  type="LINK">Paste New Values</b:Button>
				<g:FlowPanel addStyleNames="margin-top-10" ui:field="pasteNewValuesPanel" />
				<b:Alert type="DANGER" ui:field="alert" visible="false"
					title="Error" addStyleNames="margin-top-10" />
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="cancelButton" type="LINK"
					dataDismiss="MODAL">Cancel</b:Button>
				<b:Button ui:field="saveButton" type="PRIMARY"
					dataLoadingText="Saving...">Ok</b:Button>
			</b:ModalFooter>
		</b:Modal>
	</g:HTMLPanel>

</ui:UiBinder>