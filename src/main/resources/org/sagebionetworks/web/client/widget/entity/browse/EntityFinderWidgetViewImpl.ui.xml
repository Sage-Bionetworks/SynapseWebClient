<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
 	xmlns:s="urn:import:org.sagebionetworks.web.client.widget.entity.controller"
 	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">
	<b:Modal dataBackdrop="STATIC" dataKeyboard="true"
		addStyleNames="entityFinder modal-max-content topLevelZIndex">
		<b:ModalHeader closable="true">
			<b:Heading ui:field="modalTitle" size="H4" text="Find in Synapse"
				addStyleNames="displayInline" />
			<w:HelpWidget ui:field="helpWidget"
				helpMarkdown="Test PR do not merge."
				addStyleNames="margin-left-5" placement="BOTTOM" />
		</b:ModalHeader>
		<b:ModalBody>
			<bh:Paragraph ui:field="promptCopy"></bh:Paragraph>
			<w:ReactComponentDiv ui:field="entityFinderContainer" />
			<g:SimplePanel ui:field="synAlertPanel" addStyleNames="synAlertPanel" />
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="cancelButton" type="LINK">Cancel</b:Button>
			<b:Button ui:field="okButton" type="PRIMARY">Select</b:Button>
		</b:ModalFooter>
	</b:Modal>

</ui:UiBinder>