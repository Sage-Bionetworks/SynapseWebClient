<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<g:HTMLPanel>
		<!-- Tabs -->
		<div>
		    <ul class="nav nav-tabs">
		        <li class="active" ui:field="myProjectsListItem">
			    	<g:Anchor styleName="text-align-center no-background" ui:field="myProjectsLink" text="My Projects" />
			    </li>
			    <li ui:field="currentContextListItem">
			    	<g:Anchor styleName="text-align-center link no-background" ui:field="currentContextLink" text="Current Folder" />
			    </li>
			    
			    <li ui:field="myFavoritesListItem">
			    	<g:Anchor styleName="text-align-center link no-background" ui:field="myFavoritesLink" text="My Favorites" />
			    </li>
		    </ul>
		</div>
		<bh:Div ui:field="currentContextTabContents" addStyleNames="panel margin-top-15">
			<g:SimplePanel ui:field="currentContextContainer" />
		</bh:Div>
		<bh:Div ui:field="myProjectsTabContents" addStyleNames="panel margin-top-15">
			<g:SimplePanel ui:field="myProjectsContainer" />
		</bh:Div>
		<bh:Div ui:field="myFavoritesTabContents" addStyleNames="panel margin-top-15">
			<g:SimplePanel ui:field="myFavoritesContainer" />
		</bh:Div>
	</g:HTMLPanel>
</ui:UiBinder>