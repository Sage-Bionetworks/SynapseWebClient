<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<g:HTMLPanel>
		<b:Modal ui:field="infoDialog" closable="true" dataBackdrop="STATIC" dataKeyboard="false" addStyleNames="modal-fullscreen">
			<b:ModalBody>
				<b:Pre ui:field="infoDialogText" /> 		
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="okButton" type="PRIMARY"  dataDismiss="MODAL">OK</b:Button>
			</b:ModalFooter>
		</b:Modal>
		<b:Modal ui:field="awsLoginDialog" closable="true" dataBackdrop="STATIC" dataKeyboard="false" addStyleNames="modal-fullscreen">
			<b:ModalBody>
				<bh:Div ui:field="awsLoginContainer"/> 		
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="awsLoginCancelButton" dataDismiss="MODAL">Cancel</b:Button>
				<b:Button ui:field="awsLoginOkButton" dataDismiss="MODAL" type="PRIMARY">OK</b:Button>
			</b:ModalFooter>
		</b:Modal>
		
		<bh:Div ui:field="extraWidgetsContainer" />
	</g:HTMLPanel>
</ui:UiBinder>