<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

	<g:HTMLPanel>
		<g:SimplePanel ui:field="aclPanel" />
		<g:SimplePanel ui:field="markdownEditorPanel" />
		<b:Modal ui:field="infoDialog" closable="true" fade="true" dataBackdrop="STATIC" dataKeyboard="false" addStyleNames="modal-fullscreen">
			<b:ModalBody>
				<b:Pre ui:field="infoDialogText" /> 		
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="okButton" type="PRIMARY"  dataDismiss="MODAL">OK</b:Button>
			</b:ModalFooter>
		</b:Modal>

	</g:HTMLPanel>
</ui:UiBinder>