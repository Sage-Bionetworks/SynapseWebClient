<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">

	<bh:Span>
		<b:Modal ui:field="infoDialog" closable="true"
			dataBackdrop="STATIC" dataKeyboard="true"
			addStyleNames="modal-fullscreen">
			<b:ModalBody>
				<b:Pre ui:field="infoDialogText" />
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="okButton" type="PRIMARY"
					dataDismiss="MODAL">OK</b:Button>
			</b:ModalFooter>
		</b:Modal>
		<bh:Div ui:field="uploadDialogWidgetContainer" />
		<b:Modal ui:field="createVersionDialog" closable="false"
			dataBackdrop="STATIC" dataKeyboard="true" size="SMALL">
			<b:ModalBody>
				<bh:Div ui:field="createVersionJobTrackingWidgetContainer" />
			</b:ModalBody>
		</b:Modal>
		<bh:Div ui:field="extraWidgetsContainer" />
	</bh:Span>
</ui:UiBinder>