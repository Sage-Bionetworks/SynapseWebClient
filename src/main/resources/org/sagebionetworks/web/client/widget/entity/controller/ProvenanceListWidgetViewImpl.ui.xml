<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" 
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	
    <g:HTMLPanel>
        <t:Table styleName="table table-condensed break-word word-break-all" >
		    <t:TBody ui:field="provTableBody"/>
		</t:Table>
        <b:Button text="Add Synapse Reference" icon="PLUS" ui:field="addEntityButton"/>
        <b:Button text="Add External URL Reference" icon="PLUS" ui:field="addURLButton"/>
    	<g:SimplePanel ui:field="entityFinderPanel"/>
    	<g:SimplePanel ui:field="urlDialogPanel"/>
    </g:HTMLPanel>
</ui:UiBinder>  
