<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.entity.menu.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
    <b:Modal ui:field="modal" title="Add Provenance URL" closable="true" fade="true"
		dataBackdrop="STATIC" dataKeyboard="false" size="LARGE">
		<b:ModalBody>
		    <b:FormGroup>
		     	<b:FormLabel>URL</b:FormLabel>
		       	<b:TextBox ui:field="editURLField" addStyleNames="margin-left-10 margin-bottom-10 hide-resize"/>
		        <b:FormLabel for="editName">Name (Optional)</b:FormLabel>
		        <b:TextBox ui:field="editNameField" addStyleNames="margin-left-10 margin-bottom-10 hide-resize"/>
		    </b:FormGroup>
			<g:SimplePanel ui:field="synAlertPanel"/>
		</b:ModalBody>
		<b:ModalFooter>
		    <b:Button text="Cancel" ui:field="cancelButton" addStyleNames="right margin-right-5"/>
			<b:Button ui:field="saveButton" text="Add" type="PRIMARY"/>
		</b:ModalFooter>
	</b:Modal>

</ui:UiBinder>  
