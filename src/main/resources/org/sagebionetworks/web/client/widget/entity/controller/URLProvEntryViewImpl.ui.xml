<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
>
  <t:TableRow>
    <t:TableData addStyleNames="col-xs-4">
      <bh:Strong>
        Name: &nbsp;
      </bh:Strong>
      <bh:Text ui:field="urlNameField" />
    </t:TableData>
    <t:TableData addStyleNames="col-xs-7">
      <bh:Strong addStyleNames="left">
        URL: &nbsp;
      </bh:Strong>
      <b:Anchor ui:field="urlAddressField" />
    </t:TableData>
    <t:TableData addStyleNames="col-xs-1">
      <b:Button ui:field="removeButton" icon="REMOVE" />
    </t:TableData>
  </t:TableRow>
</ui:UiBinder>
