<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<b:Panel>
		<b:PanelBody>
			<b:Heading size="H5" addStyleNames="margin-bottom-0-imp">
				<bh:Text>Authorization is required to access</bh:Text>
				<bh:Strong ui:field="endpointField" addStyleNames="margin-left-5"/>
			</b:Heading>
			<bh:Italic text="Keys are used to locally sign a web request.  They are not transmitted or stored by Synapse." />
			<b:Form>
				<b:FormGroup addStyleNames="margin-top-10">
					<b:FormLabel>Access Key</b:FormLabel>
					<b:Input type="TEXT" name="username" ui:field="accessKeyField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel>Secret Key</b:FormLabel>
					<b:Input type="PASSWORD" name="password" ui:field="secretKeyField"/>
				</b:FormGroup>
			</b:Form>
		</b:PanelBody>
	</b:Panel>
</ui:UiBinder>