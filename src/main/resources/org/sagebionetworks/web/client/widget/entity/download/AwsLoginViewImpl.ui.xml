<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<b:Heading size="H4" addStyleNames="margin-bottom-0-imp">
			<bh:Text>Authorization is required to access</bh:Text>
			<bh:Strong ui:field="endpointField" addStyleNames="margin-left-5"/>
		</b:Heading>
		<b:FormGroup>
			<b:FormLabel>Access Key</b:FormLabel>
			<b:TextBox ui:field="accessKeyField" autocomplete="false"/>
		</b:FormGroup>
		<b:FormGroup>
			<b:FormLabel>Secret Key</b:FormLabel>
			<b:TextBox ui:field="secretKeyField" autocomplete="false"/>
		</b:FormGroup>
		<bh:Italic text="Note that these access keys are used to locally sign a request. They are not transmitted or stored by this website." />
	</bh:Div>
</ui:UiBinder>