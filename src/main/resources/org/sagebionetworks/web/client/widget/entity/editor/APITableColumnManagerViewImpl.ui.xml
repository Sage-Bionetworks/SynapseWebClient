<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:s="urn:import:org.sagebionetworks.web.client.widget"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	>
	<ui:with field='iconsImageBundle' type='org.sagebionetworks.web.client.IconsImageBundle'/>
	
	<bh:Div>
		<b:Panel>
			<b:PanelHeader>
	            <b:Heading size="H3" text="Columns"/>
	        </b:PanelHeader>
	        <b:PanelBody addStyleNames="margin-bottom-5-imp">
	        	<bh:Span ui:field="noColumnsUI" text="No Columns." />
	        	<s:SelectionToolbar ui:field="selectionToolbar"/>
	        	<t:Table ui:field="columnHeaders" width="100%" height="30px">
					<t:TableRow>
						<t:TableHeader width="35px"/> <!-- select checkbox -->
						<t:TableHeader width="100px" paddingRight="5">
							<bh:Span text="Renderer" />
						</t:TableHeader>
						<t:TableHeader width="120px" paddingRight="5">
							<bh:Span text="Column Name" />
						</t:TableHeader>
						<t:TableHeader width="120px" paddingRight="5">
							<bh:Span text="Display Name" />
						</t:TableHeader>
						<t:TableHeader width="75px" paddingRight="5">
							<bh:Span text="Sort" />
						</t:TableHeader>
					</t:TableRow>
				</t:Table>
	            <t:Table width="100%" ui:field="columnRenderersContainer"/>
	            <b:Button ui:field="addColumnButton" icon="PLUS" size="SMALL" addStyleNames="margin-10">Add Column</b:Button>
	        </b:PanelBody>
		</b:Panel>
	</bh:Div>
	
</ui:UiBinder>