<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<bh:Div>
			<b:FieldSet>
				<b:FormGroup>
					<b:FormLabel for="urlField">URL</b:FormLabel>
					<b:TextBox ui:field="urlField" name="urlField" b:id="urlField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:CheckBox ui:field="isRowVisibleField" text="Show row numbers"/>
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="rowNumbersColumnNameField">Row numbers column name</b:FormLabel>
					<b:TextBox ui:field="rowNumbersColumnNameField" name="rowNumbersColumnNameField" b:id="rowNumbersColumnNameField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:CheckBox ui:field="isPagingField" text="Support pagination"/>
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="pageSizeField">Page size</b:FormLabel>
					<b:TextBox ui:field="pageSizeField" name="pageSizeField" b:id="pageSizeField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="jsonResultsKeyNameField">JSON 'results' key name</b:FormLabel>
					<b:TextBox ui:field="jsonResultsKeyNameField" name="jsonResultsKeyNameField" b:id="jsonResultsKeyNameField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="cssStyleNameField">CSS style name</b:FormLabel>
					<b:TextBox ui:field="cssStyleNameField" name="cssStyleNameField" b:id="cssStyleNameField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:CheckBox ui:field="isQueryTableResults" text="Query table results?"/>
				</b:FormGroup>
				<b:FormGroup>
					<b:CheckBox ui:field="isShowIfLoggedInOnly" text="Show if logged in only?"/>
				</b:FormGroup>
			</b:FieldSet>
		</bh:Div>
		<b:Panel>
			<b:PanelBody>
				<g:SimplePanel ui:field="columnManagerContainer"/>
			</b:PanelBody>
		</b:Panel>
	</bh:Div>
	
</ui:UiBinder>