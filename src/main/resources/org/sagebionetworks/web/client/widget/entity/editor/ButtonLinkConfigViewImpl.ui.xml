<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<bh:Div addStyleNames="form-horizontal">
			<b:FieldSet>
				<b:FormGroup>
					<b:FormLabel addStyleNames="col-xs-2" text="URL" />
					<g:FlowPanel addStyleNames="col-xs-10">
						<b:TextBox ui:field="urlField" />
					</g:FlowPanel>
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel addStyleNames="col-xs-2" text="Button Text" />
					<g:FlowPanel addStyleNames="col-xs-10">
						<b:TextBox ui:field="nameField" />
					</g:FlowPanel>
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel addStyleNames="col-xs-2" text="Button Style" />
					<g:FlowPanel addStyleNames="col-xs-10">
						<b:Radio ui:field="defaultButtonStyle" name="buttonStyle" text="Light" />
						<b:Radio ui:field="infoButtonStyle" name="buttonStyle" text="Dark" />
					</g:FlowPanel>
				</b:FormGroup>
				<bh:Hr />
				<b:FormGroup>
					<b:FormLabel addStyleNames="col-xs-2" text="Preview" />
					<g:FlowPanel addStyleNames="col-xs-10">
						<b:Button ui:field="previewButton"/>
					</g:FlowPanel>
				</b:FormGroup>
			</b:FieldSet>
		</bh:Div>
	</bh:Div>
	
</ui:UiBinder>