<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<b:Row addStyleNames="margin-10">
			<b:Column size="XS_6">
				<b:FormGroup>
					<b:FormLabel>Cytoscape JS network file</b:FormLabel>
					<b:InputGroup>
						<b:TextBox ui:field="entity" autoComplete="false" enabled="false" />
						<b:InputGroupButton>
							<b:Button text="Find cyjs file" ui:field="button" icon="SEARCH" width="125px"/>
						</b:InputGroupButton>
					</b:InputGroup>
				</b:FormGroup>
			</b:Column>
			<b:Column size="XS_6" />
		</b:Row>
		<b:Row addStyleNames="margin-10">
			<b:Column size="XS_6">
				<b:FormGroup>
					<b:FormLabel>Cytoscape JS style file</b:FormLabel>
					<b:InputGroup>
						<b:TextBox ui:field="styleEntity" autoComplete="false" enabled="false" />
						<b:InputGroupButton>
							<b:Button text="Find json file" ui:field="styleButton" icon="SEARCH" width="125px"/>
						</b:InputGroupButton>
					</b:InputGroup>
				</b:FormGroup>
			</b:Column>
			<b:Column size="XS_6" />
		</b:Row>
		<b:Row addStyleNames="margin-10">
			<b:Column size="XS_6">
					<b:FormGroup>
						<b:FormLabel>Display height (px)</b:FormLabel>
						<b:TextBox placeholder="500 (Default)" ui:field="displayHeightField" />
					</b:FormGroup>
			</b:Column>
			<b:Column size="XS_6" />
		</b:Row>
	</bh:Div>
</ui:UiBinder>