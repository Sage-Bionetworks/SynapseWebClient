<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<bh:Div>
			<b:FieldSet>
				<b:FormGroup>
					<b:FormLabel>Label</b:FormLabel>
					<g:FlowPanel>
						<b:TextBox ui:field="summaryField" />
					</g:FlowPanel>
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel>Details</b:FormLabel>
					<g:FlowPanel>
						<b:TextArea ui:field="detailsField" visibleLines="10"/>
					</g:FlowPanel>
				</b:FormGroup>
			</b:FieldSet>
		</bh:Div>
	</bh:Div>
	
</ui:UiBinder>