<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:s="urn:import:org.sagebionetworks.web.client.widget"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
>
  <bh:Div>
    <bh:Div>
      <bh:Div>
        <s:SelectionToolbar
          ui:field="selectionToolbar"
          addStyleNames="displayInlineBlock"
        />
        <b:Button
          ui:field="editNoteButton"
          icon="EDIT"
          type="INFO"
          addStyleNames="right"
          size="EXTRA_SMALL"
        >
          Edit Note
        </b:Button>
      </bh:Div>
      <bh:Div ui:field="entityListContainer" />
      <b:Button
        ui:field="addEntityButton"
        icon="PLUS"
        type="INFO"
        addStyleNames="margin-10"
      >
        Add Entities
      </b:Button>
      <bh:Div ui:field="widgets" />
    </bh:Div>
  </bh:Div>
</ui:UiBinder>
