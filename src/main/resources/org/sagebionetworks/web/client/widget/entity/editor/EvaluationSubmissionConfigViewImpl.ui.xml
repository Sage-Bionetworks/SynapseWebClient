<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<bh:Div addStyleNames="form-horizontal">
			<b:Row addStyleNames="margin-10">
				<b:FormLabel addStyleNames="col-xs-4" text="Challenge Project" />
				<b:Column size="XS_6">
					<b:InputGroup>
						<b:TextBox ui:field="challengeProjectField" autoComplete="false" enabled="false" />
						<b:InputGroupButton>
							<b:Button text="Find Project" ui:field="findProjectButton" icon="SEARCH" width="125px"/>
						</b:InputGroupButton>
					</b:InputGroup>
				</b:Column>
				<b:Column size="XS_2" />
			</b:Row>
			<b:Row addStyleNames="margin-10">
				<b:FormLabel addStyleNames="col-xs-4" text="Evaluation Queue unavailable message" />
				<b:Column size="XS_8">
					<b:TextBox ui:field="unavailableMessageField" value="Join the team to submit to the challenge" />
				</b:Column>
			</b:Row>
			<b:Row addStyleNames="margin-10">
				<b:FormLabel addStyleNames="col-xs-4" text="Button text" />
				<b:Column size="XS_8">
					<b:TextBox ui:field="buttonTextField" value="Submit to challenge" />
				</b:Column>
			</b:Row>
			
		</bh:Div>
	</bh:Div>
	
</ui:UiBinder>