<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<b:Radio ui:field="challengeRadioOption" name="evaluationQueueType" text="Challenge Queue(s)" value="true" />	
		<bh:Div ui:field="challengeProjectUi"  marginLeft="30">
			<b:FormLabel text="Challenge Project" />
			<b:InputGroup>
				<b:TextBox ui:field="challengeProjectField" autoComplete="false" enabled="false" />
				<b:InputGroupButton>
					<b:Button text="Find Project" ui:field="findProjectButton" icon="SEARCH" width="125px"/>
				</b:InputGroupButton>
			</b:InputGroup>
		</bh:Div>
		<b:Radio ui:field="evaluationQueueOption" name="evaluationQueueType" text="Single Evaluation Queue" />
		<bh:Div ui:field="evaluationQueueUi" marginLeft="30" visible="false">
			<b:FormLabel text="Evaluation Queue ID" />
			<b:TextBox ui:field="evaluationQueueIdField" value="" placeholder="e.g. 9614271" />
		</bh:Div>
		<bh:Hr />
		<bh:Div addStyleNames="margin-10">
			<b:FormLabel text="Evaluation Queue unavailable message" />
			<b:TextBox ui:field="unavailableMessageField" value="Join the team to submit to the challenge" />
		</bh:Div>
		<bh:Div addStyleNames="margin-10">
			<b:FormLabel text="Button text" />
			<b:TextBox ui:field="buttonTextField" value="Submit to challenge" />
		</bh:Div>
	</bh:Div>
</ui:UiBinder>