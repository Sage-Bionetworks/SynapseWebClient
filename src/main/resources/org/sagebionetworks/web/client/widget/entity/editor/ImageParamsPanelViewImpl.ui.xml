<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<b:Panel addStyleNames="margin-top-10 margin-bottom-10">
		<b:PanelBody>
			<g:SimplePanel>
				<b:FormLabel>Alignment</b:FormLabel>
			</g:SimplePanel>
			<b:ButtonGroup>
				<b:Button ui:field="noneButton" text="None"></b:Button>
				<b:Button ui:field="leftButton" text="Left"></b:Button>
				<b:Button ui:field="centerButton" text="Center"></b:Button>
				<b:Button ui:field="rightButton" text="Right"></b:Button>
			</b:ButtonGroup>
		</b:PanelBody>
	</b:Panel>
</ui:UiBinder>