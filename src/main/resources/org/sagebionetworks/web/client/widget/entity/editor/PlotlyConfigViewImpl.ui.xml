<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div addStyleNames="margin-top-left-10">
		<b:FieldSet>
			<b:FormGroup>
				<b:FormLabel>Plot Type</b:FormLabel>
				<b:ButtonGroup>
					<b:ListBox ui:field="typeDropdownMenu"/>
				</b:ButtonGroup>
			</b:FormGroup>
			<b:FormGroup ui:field="barChartModeUI">
				<b:FormLabel>Bar Chart Mode</b:FormLabel>
				<b:ButtonGroup>
					<b:ListBox ui:field="barModeDropdownMenu"/>
				</b:ButtonGroup>
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Title</b:FormLabel>
				<b:TextBox ui:field="titleField" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>X-Axis Label</b:FormLabel>
				<b:TextBox ui:field="xAxisLabel" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Y-Axis Label</b:FormLabel>
				<b:TextBox ui:field="yAxisLabel" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Table/View</b:FormLabel>
				<b:InputGroup>
					<b:TextBox placeholder="syn123" autoComplete="false" ui:field="tableViewSynId" />
					<b:InputGroupButton>
						<b:Button ui:field="entityPickerButton" icon="SEARCH" text="" height="30px"/>
					</b:InputGroupButton>
				</b:InputGroup>
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Select X Axis Column</b:FormLabel>
				<b:InputGroup>
					<b:TextBox placeholder="x" autoComplete="false" ui:field="xAxisColumnName" />
					<b:InputGroupButton>
						<b:Button ui:field="xAxisButton" text="Find Column" height="30px"/>
					</b:InputGroupButton>
				</b:InputGroup>
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Select Y Axis Column(s)</b:FormLabel>
				<b:InputGroup>
					<bh:Div ui:field="yAxisColumnsContainer"/>
					<b:TextBox placeholder="y1" autoComplete="false" ui:field="yAxisColumnName" />
					<b:InputGroupButton>
						<b:Button ui:field="addYAxisButton" icon="PLUS" />
					</b:InputGroupButton>
					<b:Button ui:field="findYAxisButton" text="Find Column" height="30px"/>
				</b:InputGroup>
			</b:FormGroup>
			<bh:Div ui:field="showHideAdvancedButtonContainer" />
			<bh:Div ui:field="advancedUI" visible="false">
				<b:FormGroup>
					<b:FormLabel>Where Clause</b:FormLabel>
					<b:TextBox placeholder="x>2" ui:field="whereClause" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel>Group By Clause</b:FormLabel>
					<b:TextBox ui:field="groupByClause" />
				</b:FormGroup>
			</bh:Div>
		</b:FieldSet>
		<bh:Div ui:field="synAlertContainer" />
		<bh:Div ui:field="extraWidgets"/>
	</bh:Div>

</ui:UiBinder>