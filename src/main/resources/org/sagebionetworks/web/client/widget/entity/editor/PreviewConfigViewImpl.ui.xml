<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<bh:Div>
			<b:FieldSet>
				<b:Button ui:field="entityFinderButton" icon="SEARCH" text="Find Entity" />
				<b:FormGroup addStyleNames="margin-top-10">
					<b:FormLabel>Entity ID</b:FormLabel>
					<b:TextBox ui:field="entityIdField" placeholder=""/>
			    </b:FormGroup>
			    <b:FormGroup>
					<b:FormLabel>Version (Optional)</b:FormLabel>
					<b:TextBox ui:field="versionField" placeholder=""/>
			    </b:FormGroup>
			</b:FieldSet>
		</bh:Div>
	</bh:Div>
	
</ui:UiBinder>