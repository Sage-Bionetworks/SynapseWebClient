<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<b:Form type="HORIZONTAL">
			<b:FieldSet>
				<b:FormGroup>
					<b:FormLabel for="entityList" addStyleNames="col-xs-2">Entity List</b:FormLabel>
					<g:FlowPanel addStyleNames="col-xs-10">
						<b:InputGroup>
							<b:TextBox ui:field="entityListField" name="entityList" b:id="entityList" />
							<b:InputGroupButton>
								<b:Button ui:field="entityFinderButton" icon="SEARCH_PLUS" text="Find Entities" />
							</b:InputGroupButton>
						</b:InputGroup>
					</g:FlowPanel>
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="depth" addStyleNames="col-xs-2">Depth</b:FormLabel>
					<g:FlowPanel addStyleNames="col-xs-10">
						<b:TextBox ui:field="depthField" name="depth" b:id="depth" />
					</g:FlowPanel>
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="displayHeight" addStyleNames="col-xs-2">Display Height (px)</b:FormLabel>
					<g:FlowPanel addStyleNames="col-xs-10">
						<b:TextBox placeholder="275 (Default)" ui:field="displayHeightField" name="displayHeight" b:id="displayHeight" />
					</g:FlowPanel>
				</b:FormGroup>
				<b:FormGroup>
					<g:FlowPanel addStyleNames="col-xs-offset-2 col-xs-10">
						<b:CheckBox ui:field="showExpandCheckbox" text="Show Expand"/>
					</g:FlowPanel>
				</b:FormGroup>
			</b:FieldSet>
		</b:Form>
	</bh:Div>
	
</ui:UiBinder>