<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div>
		<b:FieldSet>
			<b:FormGroup>
				<b:FormLabel for="buttonText">Button text</b:FormLabel>
				<b:TextBox ui:field="buttonTextField" name="buttonText" b:id="buttonText" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Button type</b:FormLabel>
				<g:ListBox ui:field="buttonTypeListBox">
					<g:item>DEFAULT</g:item>
					<g:item>INFO</g:item>
					<g:item>PRIMARY</g:item>
					<g:item>WARNING</g:item>
					<g:item>DANGER</g:item>
					<g:item>LINK</g:item>
					<g:item>SUCCESS</g:item>
				</g:ListBox>
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="uri">Repo resource service path</b:FormLabel>
				<b:TextBox ui:field="uriField" name="uri" b:id="uri" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel>Method</b:FormLabel>
				<g:ListBox ui:field="methodListBox">
					<g:item>GET</g:item>
					<g:item>POST</g:item>
					<g:item>PUT</g:item>
					<g:item>DELETE</g:item>
				</g:ListBox>
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="requestJson">Request Body (json)</b:FormLabel>
				<b:TextArea ui:field="requestJsonField" name="requestJson" b:id="requestJson" />
			</b:FormGroup>
		</b:FieldSet>
	</bh:Div>

</ui:UiBinder>