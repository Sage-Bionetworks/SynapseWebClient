<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<b:FieldSet>
			<b:FormGroup>
				<b:FormLabel for="url">Site URL</b:FormLabel>
				<b:TextBox ui:field="urlField" name="url" b:id="url" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="heightField">Display height (px)</b:FormLabel>
				<b:TextBox ui:field="heightField" name="heightField" b:id="heightField" placeholder="400 (Default)" />
			</b:FormGroup>
		</b:FieldSet>
	</bh:Div>
</ui:UiBinder>