<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">
	<bh:Div>
		<bh:Div>
			<b:FieldSet>
				<b:FormGroup>
					<b:FormLabel for="url" addStyleNames="displayInline">Query</b:FormLabel>
					<w:HelpWidget text="" helpMarkdown="Provide a Query for any Synapse Table. Only users who have access to the Table being queried will be able to see the Table in the Wiki page." href="http://docs.synapse.org/articles/tables.html" placement="BOTTOM" />
					<g:FlowPanel>
						<b:TextBox ui:field="queryField" name="query" b:id="query" placeholder="SELECT * FROM syn12345"/>
					</g:FlowPanel>
				</b:FormGroup>
			</b:FieldSet>
		</bh:Div>
	</bh:Div>
	
</ui:UiBinder>