<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	
	<bh:Div>
		<!-- Tabs -->
		<b:NavTabs>
			<b:TabListItem ui:field="synapseTabListItem" dataTarget="#tab1" text="From Synapse File"/>
			<b:TabListItem ui:field="youtubeTabListItem" dataTarget="#tab2" text="YouTube"/>
			<b:TabListItem ui:field="vimeoTabListItem" dataTarget="#tab3" text="Vimeo"/>
		</b:NavTabs>
		<b:TabContent>
			<b:TabPane b:id="tab1" addStyleNames="margin-top-15" ui:field="tab1">
				<b:Row addStyleNames="margin-10">
					<b:Column size="XS_12">
						<b:FormLabel>Video File</b:FormLabel>
						<b:InputGroup>
							<b:TextBox ui:field="entity" autoComplete="false" enabled="false" height="34px"/>
							<b:InputGroupButton>
								<b:Button ui:field="button" icon="SEARCH" height="34px"/>
							</b:InputGroupButton>
						</b:InputGroup>
					</b:Column>
					<b:Column size="XS_12" >
						<b:Heading size="H5" ui:field="videoFormatWarning" visible="false">
							<bh:Text>We recommend using the mp4 video format for</bh:Text>
							<b:Anchor ui:field="moreInfoLink" target="_blank" addStyleNames="margin-left-5" text="cross-browser compatability" href="http://en.wikipedia.org/wiki/HTML5_video#Browser_support"/>
						</b:Heading>
					</b:Column>
				</b:Row>
			</b:TabPane>
			<b:TabPane b:id="tab2" addStyleNames="margin-top-15" ui:field="tab2">
				<b:FieldSet>
					<b:FormGroup>
						<b:FormLabel>YouTube Video URL</b:FormLabel>
						<b:TextBox ui:field="youtubeUrlField" />
					</b:FormGroup>
				</b:FieldSet>
			</b:TabPane>
			<b:TabPane b:id="tab3" addStyleNames="margin-top-15" ui:field="tab3">
				<b:FieldSet>
					<b:FormGroup>
						<b:FormLabel>Vimeo Video URL</b:FormLabel>
						<b:TextBox ui:field="vimeoUrlField" />
					</b:FormGroup>
				</b:FieldSet>
			</b:TabPane>
		</b:TabContent>
	</bh:Div>
</ui:UiBinder>