<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
>
  <bh:Span>
    <b:Modal
      ui:field="s3DirectLoginDialog"
      title="Download"
      closable="true"
      dataKeyboard="true"
    >
      <b:ModalBody>
        <bh:Div ui:field="s3DirectLoginDialogBody" />
      </b:ModalBody>
      <b:ModalFooter>
        <b:Button ui:field="s3DirectLoginDialogButton" type="PRIMARY">
          Login
        </b:Button>
        <b:Button type="LINK" dataDismiss="MODAL">Cancel</b:Button>
      </b:ModalFooter>
    </b:Modal>
    <b:Modal
      ui:field="s3DirectDownloadDialog"
      title="Ready to download"
      closable="true"
      dataKeyboard="true"
    >
      <b:ModalBody>
        <b:Button
          ui:field="s3DirectDownloadButton"
          type="PRIMARY"
          size="LARGE"
          icon="DOWNLOAD"
          width="100%"
          marginBottom="30"
        >
          Download
        </b:Button>
      </b:ModalBody>
    </b:Modal>
  </bh:Span>
</ui:UiBinder>
