<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div addStyleNames="margin-5">
		<b:Panel ui:field="formUI">
			<b:PanelHeader>
				<bh:Span ui:field="userBadgeContainer" /> 
				<bh:Italic text=" would like to gather some information from you:" addStyleNames="moveup-10"/>
			</b:PanelHeader>
			<b:PanelBody>
			    <bh:Div ui:field="rowWidgetContainer"/>
			    <b:Button ui:field="submitButton" type="PRIMARY" text="Submit" dataLoadingText="Submitting..."/>
			    <bh:Div ui:field="synAlertContainer"/>
			</b:PanelBody>
			<b:PanelFooter>
				<bh:Span text="Powered by" addStyleNames="margin-right-5" />
				<b:Anchor href="#!Help:SynapseForm" target="_blank">Synapse Forms</b:Anchor>
			</b:PanelFooter>
		</b:Panel>
		<b:Alert type="INFO" ui:field="successMessageUI" >
			<bh:Span ui:field="successMessageText"/>
		</b:Alert>
	</bh:Div>
</ui:UiBinder>