<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
>
  <g:FlowPanel addStyleNames="margin-top-15 margin-left-15 margin-right-15">
    <g:FlowPanel ui:field="adminTabContainer">
      <bh:Div>
        <bh:Span ui:field="actionMenuContainer" addStyleNames="right" />
      </bh:Div>
      <b:Container
        ui:field="challengeWidgetContainer"
        addStyleNames="align-center"
      />
      <b:Container
        ui:field="evaluationListContainer"
        addStyleNames="align-center"
      />
    </g:FlowPanel>
    <b:Container
      ui:field="evaluationEditorContainer"
      addStyleNames="align-center"
    />
  </g:FlowPanel>
</ui:UiBinder>
