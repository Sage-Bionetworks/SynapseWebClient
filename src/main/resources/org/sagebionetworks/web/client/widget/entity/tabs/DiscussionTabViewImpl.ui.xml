<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b3="urn:import:org.gwtbootstrap3.extras.toggleswitch.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	<bh:Div>
		<bh:Div ui:field="forumContainer"></bh:Div>
		<g:FlowPanel addStyleNames="help-panel">
			<b:BlockQuote styleName="panel scroll-auto">
				<b:Icon type="COMMENTS" pull="LEFT" addStyleNames="font-size-150" />
				<b:Heading text="Synapse Discussions" size="H4" />
				<bh:Text text="The Synapse team is happy to announce the release of the new Discussion feature to help your team create a record of communications within your project. We will continue to improve and extend this feature area based on your feedback. Please let us know how you're using this feature and any problems or ideas for improvements by emailing us at discussion@synapse.org."></bh:Text>
				<bh:Div addStyleNames="margin-10">
					<b:Button ui:field="learnMoreButton" text="Learn More" type="PRIMARY"  />
				</bh:Div>
			</b:BlockQuote>
		</g:FlowPanel>

	</bh:Div>
</ui:UiBinder>
