<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<b:ListItem ui:field="tabListItem" height="39px" addStyleNames="entity-tab">
			<b:Anchor ui:field="tabItem" addStyleNames="text-align-center link no-background" />
		</b:ListItem>
		<b:TabPane ui:field="tabPane" active="true" addStyleNames="row tab-background padding-top-15">
			<bh:Div ui:field="contentDiv" addStyleNames="margin-left-15 margin-right-15"/> 
		</b:TabPane>
	</bh:Div>
</ui:UiBinder>
