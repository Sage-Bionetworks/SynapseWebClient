<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<b:Panel ui:field="challengeUI">
			<bh:Div addStyleNames="highlight-title margin-left-5">
				<bh:Text text="Challenge" />
			</bh:Div>
			<bh:Div addStyleNames="margin-left-15 padding-bottom-15">
				<bh:Div>
					<b:Heading size="H5" text="Challenge ID:"
						addStyleNames="displayInlineBlock margin-right-10" />
					<b:Heading size="H5" ui:field="challengeIdHeading"
						addStyleNames="displayInlineBlock" />
				</bh:Div>
				<bh:Div>
					<bh:Div ui:field="submitToChallengeContainer" />
					<b:Heading size="H5" text="Registered participants team"
						addStyleNames="displayInlineBlock margin-right-10" />
					<b:Button ui:field="editTeamButton" type="DEFAULT"
						text="Edit" icon="EDIT" size="EXTRA_SMALL" />
					<bh:Div ui:field="teamWidgetContainer" />
				</bh:Div>
			</bh:Div>
		</b:Panel>
		<bh:Div ui:field="selectTeamModalContainer" />
		<bh:Div ui:field="widgetsContainer" />
	</bh:Div>

</ui:UiBinder>
