<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" 
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<!-- Dialog for creating a new table -->
	<b:Modal ui:field="modal" title="Edit Evaluation Queue" closable="true" dataBackdrop="STATIC" dataKeyboard="true">
		<b:ModalBody>
			<bh:Div>
				<b:FormGroup>
					<b:FormLabel text="Name" />
					<b:TextBox ui:field="nameField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="Description" />
					<b:TextBox ui:field="descriptionField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="Submission Instructions" />
					<b:TextBox ui:field="submissionInstructionsField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="Submission Receipt Message" />
					<b:TextBox ui:field="submissionReceiptField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="Created On" />
					<bh:Div ui:field="createdOnDiv" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel text="Created By" />
					<bh:Div ui:field="createdByDiv" />
				</b:FormGroup>
			</bh:Div>
			<bh:Div ui:field="synAlertContainer"/>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="cancelButton" type="LINK" text="Cancel" />
			<b:Button ui:field="saveButton" type="PRIMARY" dataLoadingText="Saving..." text="Save" />
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>