<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div>
		<b:Modal ui:field="modal" title="Select Evaluation Queue"
			closable="true" dataBackdrop="STATIC" dataKeyboard="true">
			<b:ModalBody>
				<bh:Div addStyleNames="margin-top-10" ui:field="evaluationListContainer" />
				<bh:Div addStyleNames="margin-bottom-10" ui:field="paginationWidgetContainer" />
				<bh:Div ui:field="synAlertContainer"
					addStyleNames="margin-top-10" />				
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="cancelButton" type="LINK" text="Cancel" />
				<b:Button ui:field="selectButton" type="PRIMARY"
					dataLoadingText="Loading..." text="Select Evaluation" />
			</b:ModalFooter>
		</b:Modal>
	</bh:Div>
</ui:UiBinder>