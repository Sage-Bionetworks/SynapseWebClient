<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">
	<ui:with field='sageImageBundle'
		type='org.sagebionetworks.web.client.SageImageBundle' />
	<g:FlowPanel>
		<b:Modal title="Submit to Challenge" dataBackdrop="STATIC"
			dataKeyboard="true" ui:field="dockerCommitModal"
			addStyleNames="modal-fullscreen">
			<b:ModalBody>
				<bh:Div>
					<b:FormGroup ui:field="dockerCommitListUI">
						<b:FormGroup>
							<b:FormControlStatic addStyleNames="h5">Select the
								commit below that you would like to submit:</b:FormControlStatic>
							<g:SimplePanel ui:field="dockerCommitListContainer" />
						</b:FormGroup>
					</b:FormGroup>
					<g:SimplePanel
						ui:field="dockerCommitListSynAlertPanel" />
				</bh:Div>
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button type="LINK" dataDismiss="MODAL">Cancel</b:Button>
				<b:Button ui:field="dockerCommitNextButton" type="PRIMARY"
					dataLoadingText="Processing...">Next</b:Button>
			</b:ModalFooter>
		</b:Modal>
		<b:Modal dataBackdrop="STATIC" dataKeyboard="true"
			ui:field="modal1">
			<b:ModalBody>
				<bh:Div>
					<bh:Div ui:field="formDiv" />
					<b:FormGroup ui:field="entityFinderUI">
						<b:FormGroup>
							<b:FormLabel>Select the Entity that you would like to submit:</b:FormLabel>
							<g:FlowPanel>
								<b:Button ui:field="entityFinderButton" icon="SEARCH"
									size="SMALL">Find Entity</b:Button>
								<b:Heading ui:field="selectedText" size="H6"
									addStyleNames="displayInline margin-left-10" />
							</g:FlowPanel>
						</b:FormGroup>
					</b:FormGroup>
					<b:FormGroup ui:field="evaluationListUI">
						<b:FormGroup>
							<b:FormLabel ui:field="multipleEvaluationsFormLabel">Select the challenge(s) below
								that you would like to submit to</b:FormLabel>
							<b:FormLabel ui:field="singleEvaluationFormLabel"
								visible="false">Submitting to the following challenge </b:FormLabel>
							<g:SimplePanel ui:field="evaluationListContainer" />
						</b:FormGroup>
					</b:FormGroup>
					<g:SimplePanel ui:field="challengeListSynAlertPanel" />
					<b:FormGroup ui:field="submissionNameUi">
						<b:FormLabel>Submission name (optional)</b:FormLabel>
						<b:TextBox ui:field="submissionNameField" />
					</b:FormGroup>
				</bh:Div>
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button type="LINK" dataDismiss="MODAL">Cancel</b:Button>
				<b:Button ui:field="nextButton" type="PRIMARY"
					dataLoadingText="Processing...">Next</b:Button>
			</b:ModalFooter>
		</b:Modal>
		<b:Modal dataBackdrop="STATIC" dataKeyboard="true"
			ui:field="modal2">
			<b:ModalBody>
				<bh:Div>
					<b:ButtonGroup>
						<b:Radio ui:field="isIndividualRadioButton" value="true"
							name="submitAsTeamGroup" text="I am submitting as an individual" />
						<b:Radio ui:field="isTeamRadioButton" value="false"
							name="submitAsTeamGroup" text="I am submitting as part of a team" />
					</b:ButtonGroup>
					<bh:Div ui:field="teamsUI">
						<bh:Paragraph emphasis="DANGER"
							ui:field="teamIneligibleHtml" />
						<bh:Div ui:field="availableTeamsUI">
							<b:FormGroup>
								<b:FormLabel>Select Team</b:FormLabel>
								<g:ListBox ui:field="teamComboBox"
									styleName="form-control" />
								<b:Anchor ui:field="registerMyTeamLink"
									text="Register a different team" />
							</b:FormGroup>
							<g:SimplePanel ui:field="teamSelectSynAlertPanel" />
							<b:FormGroup>
								<w:LoadingSpinner size="31px"
									ui:field="contributorsLoadingUI" />
								<b:FormLabel>Contributors</b:FormLabel>
								<b:Panel ui:field="contributorsPanel"
									addStyleNames="padding-5" />
							</b:FormGroup>
							<g:SimplePanel ui:field="contributorSynAlertPanel" />
						</bh:Div>
						<bh:Div ui:field="emptyTeamsUI">
							<bh:Paragraph emphasis="WARNING">No registered teams were
								found</bh:Paragraph>
							<b:Anchor ui:field="registerMyTeamLink2"
								text="Register a team" />
						</bh:Div>
					</bh:Div>
				</bh:Div>
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button type="LINK" dataDismiss="MODAL">Cancel</b:Button>
				<b:Button ui:field="okButton" type="PRIMARY"
					dataLoadingText="Processing...">Submit</b:Button>
			</b:ModalFooter>
		</b:Modal>
		<g:SimplePanel ui:field="registerTeamDialogContainer" />
	</g:FlowPanel>
</ui:UiBinder>
