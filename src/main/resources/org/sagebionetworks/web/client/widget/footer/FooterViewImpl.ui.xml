<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:d="urn:import:com.google.gwt.dom.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	
	<ui:with field='icons' type='org.sagebionetworks.web.client.IconsImageBundle'/>
	<ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
	
	<g:HTMLPanel>

		<!-- FOOTER -->	
		<b:Container fluid="true">
			<b:Row addStyleNames="synapse-blue-bg margin-top-10">
				<b:Column size="SM_12">
					<b:Heading size="H3" addStyleNames="margin-top-5 margin-bottom-5 color-white margin-left-10"
							text="PARTNERS"
					></b:Heading>
				</b:Column>
			</b:Row>
		</b:Container>
		<b:Container fluid="true">
			<b:Row>
				<b:Column size="SM_12">
					<g:HTMLPanel>
						<div class="margin-top-40 margin-left-15 margin-right-15">
							<div class="img-thumbnail margin-bottom-5 margin-right-5"><a href="http://www.lsdfa.org/" target="_new"><g:Image resource='{sageImageBundle.lsdfLogo}' styleName='padding-5' /></a></div>                            
							<!-- image needs to be updated <div class="img-thumbnail margin-bottom-5"><a href="http://www.cancer.gov/" target="_new"><g:Image resource='{sageImageBundle.nciLogo}' styleName='padding-5' /></a></div> -->                                                       
							<div class="img-thumbnail margin-bottom-5 margin-right-5"><a href="http://www.nhlbi.nih.gov/" target="_new"><g:Image resource='{sageImageBundle.nhlbiLogo}' styleName='padding-5' /></a></div>                                                       
							<div class="img-thumbnail margin-bottom-5 margin-right-5"><a href="http://www.sloan.org/" target="_new"><g:Image resource='{sageImageBundle.sloanLogo}' styleName='padding-5' /></a></div>                                                       
							<div class="img-thumbnail margin-bottom-5 margin-right-5"><a href="http://www.nia.nih.gov/" target="_new"><g:Image resource='{sageImageBundle.niaLogo}' styleName='padding-5' /></a></div>
							<div class="img-thumbnail margin-bottom-5 margin-right-5"><a href="http://www.nimh.nih.gov/" target="_new"><g:Image resource='{sageImageBundle.nimhLogo}' styleName='padding-5' /></a></div>
							<div class="img-thumbnail margin-bottom-5 margin-right-5"><a href="http://www.ctf.org/" target="_new"><g:Image resource='{sageImageBundle.ctfLogo}' styleName='padding-5' /></a></div>
						</div>
					</g:HTMLPanel>
				</b:Column>
			</b:Row>
		</b:Container>
		<b:Container fluid="true">
	        <b:Row addStyleNames="synapse-blue-bg margin-top-30 padding-15">
				<b:Column size="SM_4" addStyleNames="margin-top-15 margin-bottom-15">
					<b:Heading size="H6" addStyleNames="margin-top-5 margin-bottom-5 color-white margin-left-10 font-weight-bolder"
							text="Governance"
					></b:Heading>
					<b:Anchor href="static/governance/SageBionetworksSynapseTermsandConditionsofUse.pdf" text="Terms and Conditions of Use" addStyleNames="color-white margin-left-10 displayBlock"/>
					<b:Anchor href="static/governance/SynapseCommonsDataUseProcedure.pdf" text="Date Use Procedure" addStyleNames="color-white margin-left-10 displayBlock"/>
					<b:Anchor href="static/governance/SynapsePrivacyPolicy.pdf" text="Privacy Policy" addStyleNames="color-white margin-left-10 displayBlock"/>
				</b:Column>
				<b:Column size="SM_5" addStyleNames="margin-top-15 margin-bottom-15">
					<b:Heading size="H6" addStyleNames="margin-top-5 margin-bottom-5 color-white margin-left-10 font-weight-bolder"
							text="Support"
					></b:Heading>
					<b:Anchor href="#!Help:GettingStarted" text="Tutorial" addStyleNames="color-white margin-left-10 displayBlock"/>
					<b:Anchor href="http://r-docs.synapse.org/" text="R Client Documentation" addStyleNames="color-white margin-left-10 displayBlock"/>
					<b:Anchor href="http://python-docs.synapse.org/" text="Python Client Documentation" addStyleNames="color-white margin-left-10 displayBlock" />
					<b:Anchor href="http://python-docs.synapse.org/CommandLineClient.html" text="Command Line Client Documentation" addStyleNames="color-white margin-left-10 displayBlock"/>
					<b:Anchor href="https://github.com/Sage-Bionetworks/SynapseWebClient/blob/develop/src/main/java/org/sagebionetworks/web/server/servlet/SynapseClientImpl.java" text="Java Client" addStyleNames="color-white margin-left-10 displayBlock"/>
				</b:Column>
				<b:Column size="SM_3" addStyleNames="margin-top-15 margin-bottom-15">
					<b:Heading size="H6" addStyleNames="margin-top-5 margin-bottom-5 color-white margin-left-10 font-weight-bolder"
							text="Connect with us"
					></b:Heading>
					<!-- Twitter -->
					<bh:Div>
						<g:FocusPanel ui:field="twitterBox" addStyleNames="imageButton margin-left-10 displayInline">
							<b:IconStack>
								<b:Icon type="CIRCLE" stackBase="true" addStyleNames="color-white"/>
								<b:Icon type="TWITTER" stackTop="true" addStyleNames="synapse-blue"/>
							</b:IconStack>
						</g:FocusPanel>
						<b:Anchor text="Twitter" href="https://twitter.com/sagebio" addStyleNames="color-white margin-left-10" />
					</bh:Div>
					
					<!-- Facebook -->
					<bh:Div>
						<g:FocusPanel ui:field="facebookBox" addStyleNames="imageButton margin-left-10 displayInline">
							<b:IconStack>
								<b:Icon type="CIRCLE" stackBase="true" addStyleNames="color-white"/>
								<b:Icon type="FACEBOOK" stackTop="true" addStyleNames="synapse-blue"/>
							</b:IconStack>
						</g:FocusPanel>
						<b:Anchor text="Facebook" href="http://www.facebook.com/pages/Sage-Bionetworks/246302308981" addStyleNames="color-white margin-left-10" />
					</bh:Div>
				</b:Column>
			</b:Row>
		</b:Container>
		<b:Container fluid="true">
			<b:Row addStyleNames="synapse-blue-bg margin-top-10 padding-15">
	        	<b:Column size="SM_12">
					<t:Table width="100%">
						<t:TableRow>
							<t:TableData paddingRight="2" >
								<b:Anchor ui:field="copyrightYear" href="http://sagebase.org/" addStyleNames="margin-top-5 margin-bottom-5 color-white" />
							</t:TableData>
							<t:TableData paddingRight="2" addStyleNames="text-align-center">
								<b:Anchor href="mailto:synapseInfo@sagebase.org" text="Contact us" addStyleNames="color-white margin-left-10"/>	
							</t:TableData>
							<t:TableData paddingRight="2" addStyleNames="text-align-center">
								<b:Anchor href="http://creativecommons.org/licenses/by/3.0/legalcode" text="Creative Commons License" addStyleNames="color-white margin-left-10"/>
							</t:TableData>
							<t:TableData paddingRight="2" addStyleNames="text-align-right">
								<bh:Div addStyleNames="displayInline margin-left-10">
									<g:Label addStyleNames="margin-top-5 margin-bottom-5 color-white margin-left-2 displayInline" text="portal: " />
									<bh:Span ui:field="portalVersionSpan" addStyleNames="margin-top-5 margin-bottom-5 color-white" />
									<g:Label addStyleNames="margin-top-5 margin-bottom-5 color-white margin-left-10 displayInline" text="repo: " />
									<bh:Span ui:field="repoVersionSpan" addStyleNames="margin-top-5 margin-bottom-5 color-white" />
									<b:Tooltip title="Website alpha test mode">
										<b:Anchor ui:field="debugLink" text="@" addStyleNames="color-white margin-left-15"/>
									</b:Tooltip>
								</bh:Div>
							</t:TableData>
						</t:TableRow>
					</t:Table>
		    	</b:Column>
			</b:Row>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder>