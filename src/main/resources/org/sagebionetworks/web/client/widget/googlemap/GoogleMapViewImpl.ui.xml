<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  >
  <ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
  	<bh:Div>
  		<g:HTMLPanel ui:field="loadingUI">
			<g:Image styleName="displayInline" resource='{sageImageBundle.loading16}' />
			&nbsp;Loading...
		</g:HTMLPanel>
		<bh:Div ui:field="synAlertContainer"/>
		<b:Panel visible="false" ui:field="locationPanel">
			<b:Heading size="H3" ui:field="locationTitle"/>
			<bh:Div ui:field="userBadges"/>
		</b:Panel>
		<bh:Div ui:field="googleMapContainer"/>
  	</bh:Div>
	  
</ui:UiBinder>
