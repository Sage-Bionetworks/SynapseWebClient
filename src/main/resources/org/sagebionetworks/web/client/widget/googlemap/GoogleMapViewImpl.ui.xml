<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:with field='sageImageBundle'
		type='org.sagebionetworks.web.client.SageImageBundle' />
	<bh:Div>
		<bh:Div ui:field="synAlertContainer" />
		<bh:Div visible="false">
			<bh:Div ui:field="markerPopupContent">
				<b:Heading size="H4" ui:field="locationTitle" />
				<bh:Div ui:field="userBadges" />
			</bh:Div>
		</bh:Div>
		<bh:Div ui:field="googleMapContainer" height="300px">
			<bh:Div ui:field="googleMapCanvas" height="100%" width="100%"
				addStyleNames="mapping" />
		</bh:Div>
	</bh:Div>

</ui:UiBinder>
