<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />
	<ui:with field='sageImages' type='org.sagebionetworks.web.client.SageImageBundle' />
	<g:HTMLPanel>

		<div ui:field="headerDiv" class="span-24 notopmargin small-header">		    
			<div class="display-inline margin-top-3">
				<a href="#">
					<g:Image ui:field="logoLarge" resource="{sageImages.headerLarge}" styleName="margin-top-4" />					
					<g:Image ui:field="logoSmall" resource="{sageImages.logoSmall}" styleName="margin-top-4" />
				</a>				
				<g:HorizontalPanel styleName="display-inline last margin-top-4" ui:field="commandBar" />
				<g:SimplePanel ui:field="testSiteHeading" />
			</div>
			
					
		</div>

	</g:HTMLPanel>
</ui:UiBinder>