<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">

	<ui:with field='icons'
		type='org.sagebionetworks.web.client.IconsImageBundle' />
	<ui:with field='sageImages' type='org.sagebionetworks.web.client.SageImageBundle' />
	<g:HTMLPanel>

		<div class="span-24 notopmargin">
			<div class="span-13 logo">
				<a href="#">
					<g:Image resource="{sageImages.sageLogoAndTitle}" />
				</a>
			</div>
			<g:HorizontalPanel ui:field="commandBar" />
			<div class="span-24 notopmargin">
				<div class="push-16 span-8 last notopmargin" style="text-align: right;">
					<g:SimplePanel ui:field="searchBoxPanel" />
				</div>
			</div>
		</div>

	</g:HTMLPanel>
</ui:UiBinder>