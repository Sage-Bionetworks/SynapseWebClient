<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:with field='sageImages' type='org.sagebionetworks.web.client.SageImageBundle' />
	
	<bh:Div addStyleNames="header">
		<b:Row ui:field="headerDiv" addStyleNames="notopmargin synapse-blue-bg-gradient">
			<b:Column size="XS_12">
		        <bh:Div ui:field="headingPanel" addStyleNames="margin-left-15 inline-block" paddingTop="18">
		        	<g:Image ui:field="synapseLogo" url="images/logo.svg" 
		        	    addStyleNames="imageButton margin-bottom-15 margin-right-5 moveup-2" width="35px" height="35px"/>
		            <b:Heading size="H3" addStyleNames="displayInline color-white">
		            	<b:Anchor ui:field="projectHeadingAnchor" addStyleNames="color-white textDecorationNone lato notransition"/>
		        		<g:SimplePanel ui:field="projectFavoritePanel" addStyleNames="displayInline favoriteIcon margin-left-10" visible="false"/>
		        	</b:Heading>
			  	</bh:Div>
			  	<bh:Span ui:field="headerButtons" addStyleNames="right">
			  		<g:SimplePanel ui:field="loginLinkUI" addStyleNames="right margin-left-10 margin-right-10 margin-top-22 padding-left-15" visible="false">
						<b:Button ui:field="loginLink" text="&nbsp;&nbsp;&nbsp;Sign in&nbsp;&nbsp;&nbsp;" type="SUCCESS" />
					</g:SimplePanel>
					<b:DropDown ui:field="dashboardDropdown" addStyleNames="right padding-left-10 padding-right-10 padding-top-22 imageButton" visible="false" height="70px">
						<b:Anchor dataToggle="DROPDOWN" ui:field="dashboardDropdownAnchor" />
						<b:DropDownMenu ui:field="dashboardDropdownMenu">
							<b:AnchorListItem ui:field="myDashboardLink">Projects</b:AnchorListItem>
							<b:AnchorListItem ui:field="myTeamsLink">Teams</b:AnchorListItem>
							<b:AnchorListItem ui:field="myChallengesLink">Challenges</b:AnchorListItem>
							<b:AnchorListItem ui:field="trashLink" icon="TRASH_O" visible="false">Trash can</b:AnchorListItem>
							<b:Divider/>
							<b:AnchorListItem ui:field="mySettingsLink">Settings</b:AnchorListItem>
							<b:Divider/>
							<b:AnchorListItem addStyleNames="submit-feedback-command">Send Feedback</b:AnchorListItem>
							<b:AnchorListItem ui:field="documentationLink" icon="EXTERNAL_LINK" iconFixedWidth="true" iconPosition="RIGHT" href="http://docs.synapse.org">Documentation</b:AnchorListItem>
							<b:AnchorListItem href="#!SynapseForum:default">Help Forum</b:AnchorListItem>
							<b:AnchorListItem ui:field="emailSynapseSupportLink" icon="ENVELOPE_O" href="mailto:synapseinfo@sagebionetworks.org" iconFixedWidth="true" iconPosition="RIGHT">Contact Us</b:AnchorListItem>
							<b:Divider/>
							<b:AnchorListItem ui:field="logoutLink" icon="SIGN_OUT" iconFixedWidth="true" iconPosition="RIGHT">Sign out</b:AnchorListItem>
						</b:DropDownMenu>
					</b:DropDown>
					<bh:Span addStyleNames="border-left-1 right margin-top-22" height="25px"/>
					<b:DropDown ui:field="headerFavDropdown" pull="RIGHT" addStyleNames="padding-left-10 padding-right-15 padding-top-22 imageButton" visible="false" height="70px">
						<b:Anchor ui:field="headerFavAnchor" dataToggle="DROPDOWN" icon="STAR" iconSize="TIMES2" height="28px" addStyleNames="color-white" />
						<b:DropDownMenu ui:field="headerFavDropdownMenu" addStyleNames="dropdown-menu-right">
						</b:DropDownMenu>
					</b:DropDown>
				    <g:SimplePanel ui:field="searchBoxContainer" addStyleNames="right margin-left-10 margin-right-10 margin-top-22 margin-bottom-20"/>
				    <g:SimplePanel ui:field="stuAnnouncementsContainer" addStyleNames="right margin-left-10 "/>
				</bh:Span>
			</b:Column>
		</b:Row>
		<b:Alert ui:field="stagingAlert" type="WARNING" visible="false" marginTop="0">
			<b:Heading size="H4" text="This website is for testing only." addStyleNames="color-white margin-right-10" />
			<b:Heading size="H5" text="Any data created will be deleted shortly.  To do your work please visit " addStyleNames="color-white displayInline margin-right-10" />
			<b:Anchor href="https://www.synapse.org" text="www.synapse.org" addStyleNames="font-size-20" />
		</b:Alert>
	</bh:Div>
</ui:UiBinder>
