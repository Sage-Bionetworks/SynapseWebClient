<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:with field='sageImageBundle'
		type='org.sagebionetworks.web.client.SageImageBundle' />
	<g:HTMLPanel>
		<b:Icon ui:field="defaultIcon" type="SYN_USER"
			addStyleNames="font-size-150 lightGreyText" visible="true" />
		<g:HTMLPanel width="150px"
			addStyleNames="position-relative">
			<bh:Div addStyleNames="position-absolute topLevelZIndex">
				<b:Button ui:field="removePicture" icon="TIMES"
					size="EXTRA_SMALL" type="DANGER" />
			</bh:Div>
			<b:Image ui:field="image" width="150px" visible="false"
				addStyleNames="img-circle" />
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 