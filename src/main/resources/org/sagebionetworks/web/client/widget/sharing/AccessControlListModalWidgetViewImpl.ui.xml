<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">

	<!-- Dialog for creating a new table -->
	<b:Modal addStyleNames="accessControlListModal"
		ui:field="uiModal" dataBackdrop="STATIC" dataKeyboard="true"
		size="LARGE">
		<b:ModalHeader>
			<bh:Span ui:field="modalTitleContainer" />
			<w:HelpWidget
				helpMarkdown="Sharing settings determine who can access your content, and what kind of access they have. Choose people/teams and define their level of access below. &#10;&#10;_Only Administrators can add, delete, or change access levels for other people._"
				addStyleNames="margin-left-5" placement="BOTTOM"
				href="https://help.synapse.org/docs/Sharing-Settings-and-Permissions.2024276030.html" />
		</b:ModalHeader>
		<b:ModalBody>
			<g:SimplePanel ui:field="editorPanel" />
			<bh:Div ui:field="openDataUI" marginTop="30">
				<bh:Paragraph>
					<w:IconSvg addStyleNames="synapse-green displayInline margin-right-5 moveup-3" icon="checkCircle" />
					This is <bh:Strong>Open Data</bh:Strong>.&nbsp;&nbsp;Anyone on the web can download it.
				</bh:Paragraph>
			</bh:Div>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="defaultButton" type="LINK"
				dataDismiss="MODAL">Cancel</b:Button>
			<b:Button ui:field="primaryButton" type="PRIMARY"
				dataLoadingText="Saving...">Save</b:Button>
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>
