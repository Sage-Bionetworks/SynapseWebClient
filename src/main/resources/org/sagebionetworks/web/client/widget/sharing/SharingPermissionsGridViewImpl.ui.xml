<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel ui:field="mainContainer"
		addStyleNames="panel panel-default">
		<t:Table ui:field="sharingTable"
			styleName="table table-condensed">
			<t:THead>
				<t:TableRow>
					<t:TableHeader text="People" addStyleNames="col-md-9" />
					<t:TableHeader ui:field="permissionColumnHeader"
						text="Access" />
					<t:TableHeader ui:field="deleteColumnHeader"
						width="5%" />
				</t:TableRow>
			</t:THead>
			<t:TBody ui:field="tableBody" />
		</t:Table>
	</g:HTMLPanel>
</ui:UiBinder>