<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
>
  <t:TableRow height="40px">
    <t:TableData width="20px" minimumWidth="20px">
      <g:FocusPanel ui:field="iconContainer">
        <w:EntityTypeIconImpl ui:field="icon" type="table" />
      </g:FocusPanel>
    </t:TableData>
    <t:TableData
      ui:field="entityContainer"
      addStyleNames="overflow-x-hidden "
      paddingLeft="7"
    />
    <t:TableData
      width="150px"
      minimumWidth="150px"
      addStyleNames="visible-sm visible-md visible-lg overflow-x-hidden"
    >
      <g:Label ui:field="typeField" />
    </t:TableData>
    <t:TableData
      width="90px"
      minimumWidth="90px"
      addStyleNames="visible-md visible-lg text-align-right padding-right-15"
      ui:field="itemCountColumn"
    >
      <g:Label ui:field="itemCountField" />
    </t:TableData>
    <t:TableData
      width="270px"
      minimumWidth="270px"
      addStyleNames="visible-xl overflow-x-hidden"
    >
      <bh:Div ui:field="createdByField" />
    </t:TableData>
    <t:TableData
      width="270px"
      minimumWidth="270px"
      addStyleNames="visible-xl overflow-x-hidden"
    >
      <bh:Div ui:field="modifiedByField" />
    </t:TableData>
    <t:TableData
      width="165px"
      minimumWidth="165px"
      addStyleNames="visible-md visible-lg overflow-x-hidden"
    >
      <g:Label ui:field="modifiedOnField" />
    </t:TableData>
    <t:TableData
      width="165px"
      minimumWidth="165px"
      addStyleNames="visible-md visible-lg overflow-x-hidden"
    >
      <g:Label ui:field="createdOnField" />
    </t:TableData>
    <t:TableData
      width="130px"
      minimumWidth="130px"
      addStyleNames="visible-md visible-lg overflow-x-hidden"
    >
      <g:TextBox
        ui:field="idField"
        width="130px"
        addStyleNames="border-none noBackground"
        readOnly="true"
      />
    </t:TableData>
  </t:TableRow>
</ui:UiBinder>
