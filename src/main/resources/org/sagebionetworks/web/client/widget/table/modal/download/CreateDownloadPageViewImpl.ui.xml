<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div>
		<bh:Div>
			<b:FormGroup>
				<b:FormLabel for="fileType">File type</b:FormLabel>
				<b:Radio name="fileType" ui:field="commaRadio">Comma Separated Values
					(CSV)</b:Radio>
				<b:Radio name="fileType" ui:field="tabRadio">Tab Separated Values (TSV)</b:Radio>
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="headers">Contents</b:FormLabel>
				<b:CheckBox name="headers" ui:field="writeHeader">First line is the
					columns names.</b:CheckBox>
			</b:FormGroup>
			<b:FormGroup>
				<b:CheckBox ui:field="includeMetadata">Include row metadata (Row Id and Row
					Version).</b:CheckBox>
			</b:FormGroup>
		</bh:Div>
		<g:SimplePanel ui:field="trackerPanel" />
	</bh:Div>
</ui:UiBinder>