<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<ui:with field='sageImageBundle'
		type='org.sagebionetworks.web.client.SageImageBundle' />

	<g:HTMLPanel>
		<g:SimplePanel ui:field="previewPanel" />
		<g:SimplePanel ui:field="trackerPanel" />
		<bh:Div styleName="margin-top-15" />
		<b:Button ui:field="optionsButton" text="Options" />
		<bh:Div styleName="margin-top-15" />
		<g:SimplePanel ui:field="optionsPanel" visible="false" />
	</g:HTMLPanel>

</ui:UiBinder>