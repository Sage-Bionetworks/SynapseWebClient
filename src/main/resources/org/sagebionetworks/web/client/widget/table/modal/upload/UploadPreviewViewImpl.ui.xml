<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div>
		<bh:Text ui:field="previewMessage"></bh:Text>
		<g:ScrollPanel>
			<t:Table styleName="table table-striped table-condensed"
				ui:field="table">
				<t:THead>
					<t:TableRow ui:field="header">
					</t:TableRow>
				</t:THead>
				<t:TBody ui:field="body"></t:TBody>
			</t:Table>
		</g:ScrollPanel>
		<b:Alert ui:field="emptyResults" type="DANGER" visible="false"></b:Alert>
	</bh:Div>
</ui:UiBinder>