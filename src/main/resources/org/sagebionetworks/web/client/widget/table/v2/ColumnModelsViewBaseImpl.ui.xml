<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<g:HTMLPanel>
	<g:SimplePanel ui:field="viewerPanel" />
		<!-- This modal dialog will contain the editor view -->
		<b:Modal ui:field="editModal" title="Edit Columns" closable="true" fade="true"
			dataBackdrop="STATIC" dataKeyboard="true" b:id="editColumnsModal"
			width="750px">
			<b:ModalBody ui:field="columnEditorModalPanel">
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button type="PRIMARY">Save</b:Button>
				<b:Button type="DEFAULT" dataDismiss="MODAL">Cancel</b:Button>
			</b:ModalFooter>
		</b:Modal>
	</g:HTMLPanel>

</ui:UiBinder>