<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	<g:HTMLPanel>
		<b:ButtonToolBar ui:field="buttonToolbar" visible="false">
			<b:ButtonGroup>
				<b:Button ui:field="selectTogglebutton" icon="SQUARE_O" />
				<b:Button ui:field="selectDropDown" dataToggle="DROPDOWN"
					height="28px" />
				<b:DropDownMenu>
					<b:DropDownHeader>Select</b:DropDownHeader>
					<b:AnchorListItem ui:field="selectAllItem">All</b:AnchorListItem>
					<b:AnchorListItem ui:field="selectNoneItem">None</b:AnchorListItem>
				</b:DropDownMenu>
			</b:ButtonGroup>
			<b:ButtonGroup>
				<b:Button ui:field="moveUpButton" icon="ARROW_UP" enabled="false" />
				<b:Button ui:field="moveDownButton" icon="ARROW_DOWN"
					enabled="false" />
			</b:ButtonGroup>
			<b:ButtonGroup>
				<b:Button ui:field="deleteSelectedButton" icon="TRASH_O"
					enabled="false" />
			</b:ButtonGroup>
		</b:ButtonToolBar>

		<t:Table ui:field="table" styleName="table table-striped table-condensed">
			<t:THead>
				<t:TableRow>
					<t:TableHeader>
					</t:TableHeader>
					<t:TableHeader>
						<b.html:Text>Column Name</b.html:Text>
					</t:TableHeader>
					<t:TableHeader>
						<b.html:Text>Column Type</b.html:Text>
					</t:TableHeader>
					<t:TableHeader>
						<b.html:Text>Size</b.html:Text>
					</t:TableHeader>
					<t:TableHeader>
						<b.html:Text>Default Value</b.html:Text>
					</t:TableHeader>
					<t:TableHeader>
						<b.html:Text>Restrict Values</b.html:Text>
					</t:TableHeader>
				</t:TableRow>
			</t:THead>
			<t:TBody ui:field="tableBody"></t:TBody>
		</t:Table>
		<b:Button ui:field="addColumnButton" icon="PLUS" type="INFO">Add
			Column</b:Button>
		<b:Button ui:field="editColumnsButton" icon="GEAR" type="INFO">Edit
			Columns</b:Button>
	</g:HTMLPanel>

</ui:UiBinder>