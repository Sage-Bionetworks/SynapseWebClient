<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:t="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<g:HTMLPanel>
		<b:InputGroup ui:field="queryInputGroup" visible="true">
			<b:FormGroup ui:field="inputFormGroup">
				<b:TextBox ui:field="queryInput" />
			</b:FormGroup>
			<b:InputGroupButton>
				<b:Button ui:field="queryButton" icon="SEARCH" type="INFO"
					dataLoadingText="Executing...">Query</b:Button>
				<b:Button ui:field="editResultsButton" icon="GEAR" height="34px" />
				<b:Button ui:field="downloadResultsButton" icon="DOWNLOAD" height="34px" />
				<b:Button ui:field="helpButton" icon="QUESTION" type="LINK"/>
			</b:InputGroupButton>
		</b:InputGroup>
		<b:Alert ui:field="queryResultsMessage" type="DANGER" visible="false">
			<b:Button ui:field="resetButton" icon="ROTATE_LEFT" type="LINK"
				title="Reset query"></b:Button>
		</b:Alert>
	</g:HTMLPanel>
</ui:UiBinder>