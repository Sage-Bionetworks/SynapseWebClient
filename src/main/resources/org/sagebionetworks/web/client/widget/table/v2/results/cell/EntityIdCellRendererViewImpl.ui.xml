<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	  <ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle'/>
	<bh:Span>
  		<g:Image ui:field="loadingUI" visible="false" styleName="displayInline" resource='{sageImageBundle.loading16}' />
		<b:Tooltip ui:field="errorField">
		  <b:Icon  ui:field="errorIcon" visible="false" fixedWidth="true" type="EXCLAMATION_CIRCLE" addStyleNames="text-danger"/>
		</b:Tooltip>
		<b:Icon ui:field="entityIcon" visible="false" fixedWidth="true" type="ASTERISK"/>
		<b:Anchor ui:field="entityLink" target="_blank"/>
	</bh:Span>
</ui:UiBinder>