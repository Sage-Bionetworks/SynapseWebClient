<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
>
  <bh:Span addStyleNames="whitespace-nowrap">
    <bh:Span ui:field="loadingUI" visible="false">Loading...</bh:Span>
    <w:EntityTypeIcon
      ui:field="entityIcon"
      visible="false"
      styleName="margin-right-5"
      type="file"
    />
    <b:Icon
      ui:field="errorIcon"
      visible="false"
      fixedWidth="true"
      type="ASTERISK"
    />
    <b:Anchor ui:field="entityLink" target="_blank" />
  </bh:Span>
</ui:UiBinder>
