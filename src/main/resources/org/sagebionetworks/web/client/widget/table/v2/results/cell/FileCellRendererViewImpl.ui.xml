<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<ui:with field='sageImageBundle' type='org.sagebionetworks.web.client.SageImageBundle' />

	<g:HTMLPanel>
		<g:Image ui:field="loadingImage" resource='{sageImageBundle.loading16}' />
		<bh:Text ui:field="errorText" visible="false"></bh:Text>
		<b:Anchor ui:field="anchor" visible="false"/>
	</g:HTMLPanel>

</ui:UiBinder>