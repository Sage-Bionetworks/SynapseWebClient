<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b3="urn:import:org.gwtbootstrap3.extras.slider.client.ui">
	<bh:Div>
		<bh:Div addStyleNames="margin-top-15 margin-bottom-5">
			<bh:Strong ui:field="columnName"/>
		</bh:Div>
		<b:Radio ui:field="notSetRadio" name="facetcolumnslider">Not set</b:Radio>
		<b:Radio ui:field="anyRadio" name="facetcolumnslider">Any</b:Radio>
		<b:Radio ui:field="rangeRadio" name="facetcolumnslider">Range</b:Radio>
		<bh:Div ui:field="rangeUI" addStyleNames="margin-left-10" visible="false">
			<b3:RangeSlider ui:field="slider" min="0" max="100" step="5" value="[0,100]"/> 
			<bh:Span ui:field="minField" addStyleNames="margin-left-20"/><bh:Span text=":" addStyleNames="margin-left-5 margin-right-5" /><bh:Span ui:field="maxField"/>
		</bh:Div>
	</bh:Div>
</ui:UiBinder>