<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<bh:Div addStyleNames="FacetColumnResultValueWidget">
		<b:CheckBox ui:field="select" addStyleNames="moveup-1 checkbox" />
		<g:FocusPanel ui:field="focusPanel" addStyleNames="value">
			<bh:Span>
				<bh:Span ui:field="valueName"
					addStyleNames="margin-right-5 link" />
			</bh:Span>
		</g:FocusPanel>
		<bh:Span addStyleNames="lightGreyText count">
			<bh:Span ui:field="count" />
		</bh:Span>
	</bh:Div>
</ui:UiBinder>