<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">
	<g:HorizontalPanel verticalAlignment="ALIGN_MIDDLE">
		<b:CheckBox ui:field="select" addStyleNames="moveup-1"/>
		<g:FocusPanel ui:field="valueNameContainer">
			<bh:Span ui:field="valueName" addStyleNames="margin-right-5 link"/>
		</g:FocusPanel>
		<g:FocusPanel ui:field="countContainer">
			<bh:Span addStyleNames="lightGreyText imageButton">
				<bh:Text>(</bh:Text>
				<bh:Span ui:field="count"/>
				<bh:Text>)</bh:Text>
			</bh:Span>
		</g:FocusPanel>
		
	</g:HorizontalPanel>
</ui:UiBinder>