<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<t:TableRow>
		<t:TableData>
			<b:CheckBox ui:field="select" />
		</t:TableData>

		<t:TableData>
			<b:FormGroup ui:field="nameGroup">
				<b:TextBox ui:field="name" placeholder="Column Name" />
				<b:HelpBlock ui:field="nameHelp" />
			</b:FormGroup>
		</t:TableData>
		<t:TableData>
			<g:ListBox ui:field="type" width="100px">
			  <g:item>String</g:item>
			  <g:item>Boolean</g:item>
			  <g:item>Integer</g:item>
			  <g:item>Double</g:item>
			  <g:item>Date</g:item>
			  <g:item>File</g:item>
			  <g:item>Entity</g:item>
			  <g:item>Link</g:item>
			  <g:item>LargeText</g:item>
			</g:ListBox>
		</t:TableData>
		<t:TableData>
			<b:FormGroup ui:field="sizeGroup">
				<b:TextBox ui:field="maxSize" placeholder="Size" width="75px" />
				<b:HelpBlock ui:field="sizeHelp" />
			</b:FormGroup>
		</t:TableData>
		<t:TableData>
			<g:SimplePanel ui:field="defaultPanel" />
		</t:TableData>
		<t:TableData>
			<b:TextBox ui:field="restrictValues" placeholder="value1, value2, value-n..." />
		</t:TableData>
		<t:TableData>
			<g:ListBox ui:field="facet" width="100px">
			  <g:item>None</g:item>
			  <g:item>Values</g:item>
			  <g:item>Range</g:item>
			</g:ListBox>
		</t:TableData>
	</t:TableRow>
</ui:UiBinder>