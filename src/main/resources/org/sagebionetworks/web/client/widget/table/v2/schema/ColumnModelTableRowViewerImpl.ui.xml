<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">

	<t:TableRow>
		<t:TableData>
			<b:CheckBox ui:field="select" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="name" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="type"
				width="100px" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="maxSize"
				width="75px" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="defaultValue" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="restrictValues" />
		</t:TableData>
		<t:TableData>
			<b:FormControlStatic ui:field="facetType" />
		</t:TableData>
	</t:TableRow>
</ui:UiBinder>