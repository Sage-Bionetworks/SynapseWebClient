<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
>
  <t:TableRow>
    <t:TableData>
      <b:CheckBox ui:field="select" />
    </t:TableData>
    <t:TableData>
      <b:FormControlStatic ui:field="id" />
    </t:TableData>
    <t:TableData>
      <b:FormControlStatic ui:field="name" />
    </t:TableData>
    <t:TableData>
      <b:FormControlStatic ui:field="type" width="100px" />
    </t:TableData>
    <t:TableData>
      <b:FormControlStatic ui:field="maxSize" width="75px" />
    </t:TableData>
    <t:TableData ui:field="maxListLengthTd">
      <b:FormControlStatic ui:field="maxListLength" width="75px" />
    </t:TableData>
    <t:TableData>
      <b:FormControlStatic ui:field="defaultValue" />
    </t:TableData>
    <t:TableData>
      <b:FormControlStatic ui:field="restrictValues" />
    </t:TableData>
    <t:TableData>
      <b:FormControlStatic ui:field="facetType" />
    </t:TableData>
  </t:TableRow>
</ui:UiBinder>
