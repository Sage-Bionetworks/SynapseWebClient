<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
>
  <g:HTMLPanel>
    <t:Table ui:field="table" styleName="table table-striped table-condensed">
      <t:THead>
        <t:TableRow>
          <t:TableHeader />
          <t:TableHeader ui:field="columnIdTableHeader">
            <b.html:Text>Column ID</b.html:Text>
          </t:TableHeader>
          <t:TableHeader>
            <b.html:Text>Column Name</b.html:Text>
          </t:TableHeader>
          <t:TableHeader>
            <b.html:Text>Column Type</b.html:Text>
          </t:TableHeader>
          <t:TableHeader>
            <b.html:Text>Size</b.html:Text>
          </t:TableHeader>
          <t:TableHeader ui:field="maxListLengthHeader">
            <b.html:Text>Max List Length</b.html:Text>
          </t:TableHeader>
          <t:TableHeader>
            <b.html:Text>Default Value</b.html:Text>
          </t:TableHeader>
          <t:TableHeader>
            <b.html:Text>Restrict Values</b.html:Text>
          </t:TableHeader>
          <t:TableHeader>
            <b.html:Text>Facet</b.html:Text>
          </t:TableHeader>
        </t:TableRow>
      </t:THead>
      <t:TBody ui:field="tableBody" />
    </t:Table>
    <b:Button ui:field="editColumnsButton" icon="GEAR" type="INFO">
      Edit
			Schema
    </b:Button>
    <b.html:Div ui:field="editorContainer" />
  </g:HTMLPanel>
</ui:UiBinder>
