<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<bh:Div>
		<b:FieldSet>
			<b:FormGroup>
				<b:FormGroup>
					<b:FormLabel>Team</b:FormLabel>
					<g:SimplePanel ui:field="suggestBoxPanel" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="joinButtonText">Wiki text for non-members</b:FormLabel>
					<b:TextBox ui:field="joinButtonField"
						name="joinButtonText" b:id="joinButtonText" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="memberMessageField">Wiki text for members</b:FormLabel>
					<b:TextBox ui:field="memberMessageField"
						name="memberMessageField" b:id="memberMessageField" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="joinSuccessText">"Successfully joined" notification
						wiki text</b:FormLabel>
					<b:TextBox ui:field="joinSuccessField"
						name="joinSuccessText" b:id="joinSuccessText" />
				</b:FormGroup>
				<b:FormGroup>
					<b:FormLabel for="requestOpenInfoText">"Team request pending" wiki text</b:FormLabel>
					<b:TextBox ui:field="requestOpenInfoField"
						name="requestOpenInfoText" b:id="requestOpenInfoText" />
				</b:FormGroup>
				<b:FormGroup>
					<b:CheckBox ui:field="isChallengeCheckbox"
						text="Is this a challenge sign-up?" />
				</b:FormGroup>
				<b:FormGroup>
					<b:CheckBox ui:field="isPromptForMessageCheckbox"
						text="Allow joining user to provide a message?" />
				</b:FormGroup>
			</b:FormGroup>
		</b:FieldSet>
	</bh:Div>

</ui:UiBinder>