<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:w="urn:import:org.sagebionetworks.web.client.widget">

	<g:HTMLPanel>
		<b:Form visible="false" ui:field="form">
			<b:Input ui:field="fileInput" type="FILE" />
		</b:Form>
		<b:Button addStyleNames="margin-right-5"
			ui:field="uploadbutton" icon="UPLOAD">Upload</b:Button>
		<bh:Span ui:field="uploadedFileNameField" />

		<w:LoadingSpinner ui:field="loadingUI" size="16px"
			addStyleNames="margin-right-5" visible="false" />

		<bh:Div styleName="margin-top-15" />
		<b:Progress ui:field="progressContainer" visible="false">
			<b:ProgressBar ui:field="progressBar" type="INFO"
				percent="0" text="0%" />
		</b:Progress>
		<bh:Div ui:field="synAlertContainer" />
		<!-- dataKeyboard must be false in this case since clicking ESC will also 
			close the parent modal -->
		<b:Modal ui:field="previewModal" closable="false"
			dataBackdrop="STATIC" dataKeyboard="false"
			addStyleNames="topLevelZIndex" title="Crop Image">
			<b:ModalBody ui:field="previewModalBody">
			</b:ModalBody>
			<b:ModalFooter>
				<b:Button ui:field="cancelCropButton" type="LINK">Cancel</b:Button>
				<b:Button ui:field="saveCropButton" type="PRIMARY">Save</b:Button>
			</b:ModalFooter>
		</b:Modal>
	</g:HTMLPanel>
</ui:UiBinder>