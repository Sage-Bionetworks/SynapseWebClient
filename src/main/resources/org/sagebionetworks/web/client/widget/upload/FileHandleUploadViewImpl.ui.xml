<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html">

	<g:HTMLPanel>
		<b:Form visible="false" ui:field="form">
			<b:Input ui:field="fileInput" b:id="fileInputWidget" type="FILE" />
		</b:Form>
		<b:Button addStyleNames="margin-right-5" ui:field="uploadbutton" icon="UPLOAD">Upload</b:Button>
		<bh:Span ui:field="uploadedFileNameField"/>
		<bh:Div styleName="margin-top-15" />
		<b:Progress ui:field="progressContainer" visible="false">
			<b:ProgressBar ui:field="progressBar" type="INFO"
				percent="1" text="1%" />
		</b:Progress>
		<b:Alert ui:field="alert" visible="false" type="DANGER"/>
	</g:HTMLPanel>

</ui:UiBinder>