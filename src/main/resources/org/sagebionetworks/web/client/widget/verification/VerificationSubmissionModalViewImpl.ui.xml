<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:a="urn:import:org.sagebionetworks.web.client.widget.table.v2"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table">
	<b:Modal ui:field="dialog" closable="true" fade="true" dataBackdrop="STATIC" dataKeyboard="false" addStyleNames="modal-fullscreen">
		<b:ModalBody>
			<b:Alert ui:field="reasonAlert" type="DANGER" addStyleNames="margin-bottom-10 margin-top-10">
				<bh:Strong addStyleNames="margin-10">Reason</bh:Strong>
				<bh:Text ui:field="reasonAlertText" />
			</b:Alert>
			<bh:Div ui:field="wikiPageContainer" />
			<bh:Div ui:field="publicallyVisible" addStyleNames="bg-primary padding-10 rounded">
				<b:Heading size="H4" text="Publicly visible" addStyleNames="color-white"/>
				<b:Row>
					<b:Column size="SM_6">
						<b:FormGroup>
							<b:FormLabel>First name</b:FormLabel>
							<bh:Div addStyleNames="greyBackground rounded">
								<b:TextBox ui:field="firstName" enabled="false"/>
							</bh:Div>
						</b:FormGroup>
					</b:Column>
					<b:Column size="SM_6">
						<b:FormGroup>
							<b:FormLabel>Last name</b:FormLabel>
							<bh:Div addStyleNames="greyBackground rounded">
								<b:TextBox ui:field="lastName" enabled="false"/>
							</bh:Div>
						</b:FormGroup>
					</b:Column>
				</b:Row>
				<b:Row>
					<b:Column size="SM_6">
						<b:FormGroup>
							<b:FormLabel>Affiliation</b:FormLabel>
							<bh:Div addStyleNames="greyBackground rounded">
								<b:TextBox ui:field="currentAffiliation" enabled="false"/>
							</bh:Div>
						</b:FormGroup>
					</b:Column>
					<b:Column size="SM_6">
						<b:FormGroup>
							<b:FormLabel>Location</b:FormLabel>
							<bh:Div addStyleNames="greyBackground rounded">
								<b:TextBox ui:field="location" enabled="false"/>
							</bh:Div>
						</b:FormGroup>
					</b:Column>
				</b:Row>
				<b:Row>
					<b:Column size="SM_12">
						<b:FormGroup>
							<b:FormLabel>ORC ID</b:FormLabel>
							<bh:Div>
								<b:Anchor ui:field="orcIdAnchor" addStyleNames="color-white" target="_blank" />
							</bh:Div>
						</b:FormGroup>
					</b:Column>
				</b:Row>
			</bh:Div>
			<bh:Div ui:field="actOnly" addStyleNames="bg-warning padding-10 rounded margin-top-10">
				<b:Heading size="H4" text="Visible to the Sage Bionetworks Access and Compliance Team only"/>
				<b:Row>
					<b:Column size="SM_12">
						<b:FormGroup>
							<b:FormLabel>Email</b:FormLabel>
							<bh:Div ui:field="emailAddresses"/>
						</b:FormGroup>
					</b:Column>
				</b:Row>
				<b:FormGroup>
					<b:FormLabel>Documentation (e.g. a letter from a signing official at your institution using your institution's letterhead)</b:FormLabel>
					<b:Panel ui:field="filesContainer" addStyleNames="padding-10"/>
				</b:FormGroup>
			</bh:Div>
			<bh:Div ui:field="synAlertContainer" addStyleNames="margin-top-10"/>
			<bh:Div ui:field="promptModalContainer"/>		
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="submitButton" type="PRIMARY">Submit</b:Button>
			<b:Button ui:field="okButton" type="PRIMARY" dataDismiss="MODAL">OK</b:Button>
			<b:Button ui:field="approveButton" type="SUCCESS">Approve</b:Button>
			<b:Button ui:field="rejectButton" type="DANGER">Reject</b:Button>
			<b:Button ui:field="suspendButton" type="DANGER">Suspend</b:Button>
			<b:Button ui:field="deleteButton" type="DANGER">Delete</b:Button>
			<b:Button ui:field="cancelButton" >Cancel</b:Button>
		</b:ModalFooter>
	</b:Modal>

	
</ui:UiBinder>